    if (empty(field[descript].text) = 0) then
       var descCurr as string
       descCurr = field[descript].text
       perform state.thing[raFGNarPoi].amendthing[description,field[abSumm].text]
       if (tagcount[Custom.Break] = 0) then
          perform state.thing[raFGNarPoi].amendthing[description,state.thing[raFGNarPoi].field[descript].text & " " & descCurr]
       else
       #appenddesc[raFGNarPoi,descCurr]
      endif
    else
      perform state.thing[raFGNarPoi].amendthing[description,field[abSumm].text]
    endif]]></eval>
    <eval phase="Final" priority="20000" index="2"><![CDATA[      ~ If we're disabled, do nothing
          doneif (tagis[Helper.SpcDisable] <> 0)
    
    ~if we have a selected weapon, then add " plus/and poison" to the selected weapon
          call fTargetFoc
          doneif (state.isfocus = 0) 
    
       var len as number
       var str as string
       str = focus.field[wDamExtra].text
       len = length(str)
    
      if (len = 0) then
         #extradamage[focus," plus poison",field[name].text]
      else
         #extradamage[focus," and poison",field[name].text]
      endif]]></eval>
    </thing>
  <thing id="raFGNarSpi" name="Poison Spines" description="Any character who attacks the narekhin with an unarmed attack or light weapon, or grapples with the sahuagin, has a chance of being poisoned." compset="RaceSpec">
