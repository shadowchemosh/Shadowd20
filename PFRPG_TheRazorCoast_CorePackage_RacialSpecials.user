  <thing id="raFGIchFle" name="Ichthyic Flesh" description="Ichthyic flesh requires moisture to remain supple. If a creature with ichthyic flesh does not spend at least 10 minutes per day immersed in water, its skin grows dry and cracks, dealing 1d4 points of Dexterity damage for each day it goes without immersion and cannot be healed naturally without exposure to the necessary water requirement." compset="RaceSpec" summary="You must spend at least 10 minutes per day immersed in water or take 1d4 Dex dmg.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGRouSki" name="Rough Skin" description="A sharkin deals an additional +1 damage with his unarmed strikes." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGAquAff" name="Aquatic Affinity" description="Sharkin gain a +2 racial bonus to Handle Animal and wild empathy checks when interacting with aquatic creatures." compset="RaceSpec">
