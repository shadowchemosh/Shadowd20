  <thing id="raGRRvFvPr" name="Favored Prey" description="A ravenous creature gains a +2 bonus on damage rolls against creatures of its own type and a +2 bonus on Bluff, Perception, Sense Motive, and Survival checks when using these skills against creatures of this type. Against a creature of its own race, each of the bonuses increases to +4." compset="RaceSpec">
    <tag group="SpecType" tag="Attack"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="PreAttr" priority="1000" index="2"><![CDATA[~ get type
    
    var typeStr as string
    
    typeStr = lowercase(hero.tagnames[Type.?,"+"] & "s")
    
    #situational[hero.child[skBluff],"+2 vs. " & typeStr,field[name].text]
    #situational[hero.child[skPercep],"+2 vs. " & typeStr,field[name].text]
    #situational[hero.child[skSenseMot],"+2 vs. " & typeStr,field[name].text]
    #situational[hero.child[skSurvival],"+2 vs. " & typeStr,field[name].text]]]></eval>
    <eval phase="First" priority="500"><![CDATA[~ get race name
    
    var raceStr as string
    
    raceStr = lowercase(hero.findchild[BaseRace].field[name].text & "s")
    
    #situational[hero.child[skBluff],"+4 vs. " & raceStr,field[name].text]
    #situational[hero.child[skPercep],"+4 vs. " & raceStr,field[name].text]
    #situational[hero.child[skSenseMot],"+4 vs. " & raceStr,field[name].text]
    #situational[hero.child[skSurvival],"+4 vs. " & raceStr,field[name].text]]]></eval>
    </thing>
  <thing id="raGRRvHgSA" name="Hungry Special Attacks" description="Any creature of the same kind as the ravenous creature takes a -2 penalty on saving throws against the ravenous creature&#146;s spells and special attacks." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raGRRvRvBd" name="Ravenous Body" description="A ravenous creature can eat other food, but only the flesh of its own type satiates its hunger. It must feed upon the flesh of a creature of its own type at least once every three days or it suffers the effects of starvation. When the ravenous creature&#146;s nonlethal damage due to starvation equals its Constitution score, it dies." compset="RaceSpec">
    <tag group="SpecType" tag="Weakness"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raGRRvSpri" name="Sprint" description="Once per hour, a ravenous creature can move at ten times its normal speed when it makes a charge." compset="RaceSpec">
    <fieldval field="trkMax" value="1"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="Hour"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raGRRvAgel" name="Ageless" description="A ravenous creature does not age." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raxTCCldSi" name="Cloud Sight" description="A thunder child can see through clouds, gases, fogs, mists, and smoke as though they didn&#146;t inhibit vision. Creatures and objects do not gain concealment from a thunder child due to such conditions. A thunder child may use this ability while gaseous." compset="RaceSpec">
    <tag group="SpecType" tag="Sense"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raxTCEleAu" name="Electric Aura" description="A thunder child emits an aura of electricity that can wound nearby creatures. Any creature within 10 feet of a thunder child must succeed on a Reflex save (DC 10 + 1/2 thunder child&#146;s character level + thunder child&#146;s Con modifier) each round that it remains within range or take 1d6 points of electricity damage. A thunder child can suppress or resume the use of this ability as a free action." compset="RaceSpec">
    <tag group="StandardDC" tag="aCON"/>
    <tag group="SpecType" tag="Aura"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="Final"><![CDATA[field[livename].text = "Electric Aura (10 ft.)"]]></eval>
    </thing>
  <thing id="xFGImmGas" name="Immunity to Gas, Fog or Cloud based attacks" description="Immune to gas, fod or cloud based attacks." compset="Ability">
    <tag group="SpecType" tag="Immune"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raxTCBrtWe" name="Breath Weapon" description="Once every 1d4 rounds, a thunder child can breathe a 5-foot-wide line of lightning 100 feet long plus 10 feet per 2 character levels it possesses. If the thunder child wishes, it may breathe a line half as long but 10 feet wide. Every creature in the area must make a Reflex save (DC 10 + 1/2 thunder child&#146;s character level + thunder child&#146;s Con modifier) or take 1d6 points of electricity damage per 2 character levels the thunder child possesses (minimum 1d6). Success halves the damage." compset="RaceSpec">
    <tag group="StandardDC" tag="aCON"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="Final" priority="2000"><![CDATA[var dam as number
    dam = hero.tagcount[Hero.HitDice]
    dam = round(dam/2,0,-1)
    dam = maximum(dam,1)
    
    field[livename].text = "Breath Weapon (" & dam & "d6)"]]></eval>
    </thing>
  <thing id="raxTCElecG" name="Electric Grapple" description="Any creature grappling a thunder child takes 2d6 points of electricity damage each round that the grapple is maintained." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Defense"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raxTCAirBr" name="Air Breather" description="A thunder child can breathe air even if the base creature breathed only water" compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raxTCLgtJu" name="Lightning Jump" description="When a thunder child breathes lightning, it may teleport with no chance of error to any location within the area of the lightning attack as a free action. It must make this jump immediately after using its breath weapon." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raDWWeakCa" name="Weakness to Captain&apos;s Mast" description="The sky kraken respects the power of Aizan. It senses that the artifact&apos;s supernatural power comes from her, and will not challenge the owner. The user of the mast can attack the sky kraken with impunity, but the creature will not retaliate against that individual." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="User" tag="NoAutoName"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="SpecType" tag="Weakness"/>
    </thing>
  <thing id="raFGBlkLee" name="Black Leech" description="An implated black leech grants a +2 enhancement bonus to Constitution score and the Toughness feat. However, the implated are unable to resist any spell cast upon him by the Engineer." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="AbilType" tag="Super"/>
    </thing>
  <thing id="raFGDnkShB" name="Siphoning Bite" description="A dunkleosteus can open its giant mouth in a fraction of a second, creating a vortex that draws a target in. When underwater, a dunkleosteus gains an additional 5 feet of reach with its bite attack and a +2 bonus on combat maneuver checks when grappling." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="SpecType" tag="Attack"/>
    <eval phase="PostAttr">hero.child[manGrapple].field[manCMB].value += 2</eval>
    </thing>
  <thing id="raFGJBAcid" name="Acid Spit" description="Installed within each jade bat is a strange shard of green glass that is actually a wand of acid splash containing 20+3d10 charges. Using the magic of this wand, the jade bat can spit a gob of acid up to 30 feet as a ranged touch attack (+7 ranged touch) dealing 2d4 points of damage. Each attack drains one charge from the wand." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="SpecType" tag="Attack"/>
    </thing>
  <thing id="raFGJBIllR" name="Illusory Recording" description="As a swift action, a jade bat can record both sights and sounds as three dimensional illusions, archiving everything that occurs within a 30-foot radius of the bat onto a small alien gemstone worth 120 gp embedded in its body. The jade bat can record up to 1 hour of illusions. Starting and stopping playback of these recordings is a swift action. Removing a gemstone eye or installing a gemstone eye into a clockwork spy requires a DC 25 Disable Device check as a full-round action-failure does not damage the gemstone but does erase any recorded images and sounds on the gemstone. (Engineer need not make this check since it created the bats.) Once a jade bat begins recording, it cannot cease recording. Likewise, it cannot record sound and images onto a gemstone that already contains a recording." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="AbilType" tag="Super"/>
    </thing>
  <thing id="raFGBLCurs" name="Curse of the Black Spot" description="Any individual reduced to 0 or fewer hit points by the leech swarm has a black leech burrow into their flesh. From then on, they are under the control of the Engineer and obey its commands. This effect may only be removed by either digging out the leech or by casting the spell {i}remove curse{/i} (DC 18). Manually digging out the leech is a brutal and painful process that requires a successful DC 20 Heal check and inflicts 1d4 points of Constitution damage." compset="RaceSpec">
    <tag group="AbilType" tag="Super"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="SpecType" tag="Attack"/>
    </thing>
  <thing id="rcFGAOVivB" name="Vivisection Blade" description="Replace slam attack with blade." compset="RaceCustom">
    <usesource source="CampRazorC"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="AllowRCust" tag="rAnimObjec"/>
    <tag group="CustomCost" tag="1"/>
    <tag group="Helper" tag="SpecUp"/>
    <bootstrap thing="raFGVivVcW"></bootstrap>
    <eval phase="PreLevel" priority="9000"><![CDATA[foreach pick in hero from BaseNatWep where "!wCategory.Range?"
      perform eachpick.tagreplace[wType.B,wType.S]
      eachpick.field[livename].text = "Vivisection Blade"
      eachpick.field[wDamExtra].text = " plus vicious wound"
      perform eachpick.tagreplace[wMain.?,wMain.1d8_6]
      nexteach]]></eval>
    </thing>
  <thing id="raFGVivVcW" name="Vicious Wound" description="The damage the Vivisectionist deals with its blades inflicts persistent wounds that deal 2 points of bleed damage. Bleeding resulting from these vicious wounds is particularly difficult to stanch. A successful DC 17 Heal check stops the bleeding, and any attempt to heal a creature suffering from a vicious wound must succeed on a DC 16 caster level check or the spell does not function. Success indicates the healing works normally and stops all bleed effects on the victim." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="SpecType" tag="Attack"/>
    </thing>
  <thing id="raFGHawBRa" name="Blind Rage" description="Stripped of reason and rational thought, hawanis can fly into an animalistic fury. A hawani can enter a blind rage as a free action. While in blind rage, a hawani gains a +6 morale bonus to Strength. In addition, it takes a -3 penalty to Armor Class. There is no limit to the number of rounds a hawani may spend in blind rage, however, a hawani takes 1d3 points of nonlethal damage every round it remains in a blind rage. Blind rage usually ends when a hawani falls unconscious from nonlethal damage, though it may voluntarily end it if there are no opponents left to fight." compset="RaceSpec">
    <fieldval field="actName" value="Blind Rage"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="PreAttr" priority="1000">doneif (field[abilActive].value = 0)
    
    #applybonus[BonMorale,hero.child[aSTR],6]
    #applypenalty[Penalty,hero.child[ArmorClass],-3]</eval>
    </thing>
  <thing id="raFGHaDkDr" name="Hawanapoki&apos;s Dark Dream" description="A hawani&apos;s bite saps consciousness from any victim it strikes. The first time a hawani hits a foe with its bite attack, the victim takes 1d4 points of Wisdom damage and falls into a deep sleep as if affected by {i}deep slumber{/i} unless he succeeds on a DC 13 Will save. A creature that successfully saves is not subject to the same hawani&apos;s dark dream for 24 hours. This is a mind-affecting effect. The save DC is Charisma-based." compset="RaceSpec">
    <tag group="SpecType" tag="Attack"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGHawThr" name="Thrall" description="Hawanis gain immunity to all mind-affecting effects but are under the complete domination of their qlippoth master, as if under the influence of {i}dominate monster{/i} spell. In addition, hawanis can communicate telepathically with any qlippoth within a 100-foot range. A hawani can be freed of its servitude with a {i}break enchantment{/i}, {i}miracle{/i}, or {i}wish{/i} spell. If successful, the hawani immediately flies into blind rage and attacks the nearest living creature until it is incapacitated or falls unconscious. Once freed, the hawani is immune to any qlippoth&apos;s mind-affecting effects for the next 24 hours." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <bootstrap thing="xImmMind"></bootstrap>
    </thing>
  <thing id="raFGAmorph" name="Amorphous" description="The creature&#146;s body is malleable and shapeless. It is immune to precision damage (like sneak attacks) and critical hits." compset="RaceSpec">
    <tag group="SpecType" tag="Defense"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <bootstrap thing="xImmPrec"></bootstrap>
    <bootstrap thing="xImmCrit"></bootstrap>
    </thing>
  <thing id="raFGNausPr" name="Nauseating Presence" description="Anyone within 30 ft. of you must succeed at a Fortitude save or become nauseated. Anyone who makes their save is immune to the nauseating presence for 24 hours.  The save DC is Constitution based." compset="RaceSpec" summary="Anyone within 30 ft. of you must succeed at a Fortitude save or become nauseated.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Aura"/>
    <tag group="StandardDC" tag="aCON"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGSumRoa" name="Summon Roachlord Swarm" description="You may summon 1d4 swarms of cockroaches as a standard action at will.  You can only control up to four swarms at any given time. You are immune to the swarm damage and nauseating effect of any swarm under your control." compset="RaceSpec" summary="You may summon 1d4 swarms of cockroaches and control up to 4 swarms.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="SpellLike"/>
    <tag group="SpecType" tag="SpellLike"/>
    <tag group="Usage" tag="AtWill"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGShaEmp" name="Shark Empathy" description="In any form, fel lycanthrope dire shark can communicate and empathize with normal or dire sharks. This gives then a +4 racial bonus on checks when influencing the animal&apos;s attitude and allows the communication of simple concepts and (if the animal is friendly) commands, such as &quot;friend,&quot; &quot;foe,&quot; &quot;flee,&quot; and &quot;attack.&quot;" compset="RaceSpec" summary=" +4 racial bonus on checks when influencing a shark&apos;s attitude.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="Final" priority="20000"><![CDATA[
    ~ Influence sharks
    #situational[hero.child[skHandleAn],"+4 bonus vs sharks.",field[name].text]]]></eval>
    </thing>
  <thing id="raFGMalFre" name="Frenzy" description="A Maldraht maht is constantly in a state similar to a barbarian&apos;s rage. The creature cannot cast spells, use spell-like abilities, or use Intelligence-based skills. In addition, the creature forsakes ranged weapons, instead seeking to sink its claws and teeth into flesh. It will attack on sight any creature not under the affects of maht." compset="RaceSpec" summary="You are constantly in a state similar to a barbarian&apos;s rage which limits your actions.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGImDisA" name="Disease Aura" description="Living creatures of less than 5 HD in a 30 - foot radius of the imandwa must succeed on a Fortitude save or become nauseated. Creatures with 5 HD or more must succeed at a Fortitude save or be sickened for a number of rounds equal to the imandwa&apos;s Hit Dice. A creature that successfully saves cannot be affected again by the same imandwa&apos;s aura for 24 hours. A creature that rolls a natural 1 against this aura (the d20 shows a &quot;1&quot;) catches grindblight." compset="RaceSpec" summary="Living creatures within 30ft you make Fort save or become nauseated or sickened.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Aura"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGImFesD" name="Festering Damage" description="Any creature damaged by an imandwa (even by a spell cast by the imandwa) must succeed on a Fortitude save or catch grindblight. There is no onset period for this form of the disease, and the save DC is based on the imandwa&apos;s special attack save DC, rather than the normal DC for the disease." compset="RaceSpec" summary="All your attacks including spells can cause grindblight disease.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="Final" priority="20000"><![CDATA[~ Display extra damage on all weapons
    foreach pick in hero from BaseWep
       #extradamage[eachpick," plus festering damage",field[name].text]
    nexteach]]></eval>
    </thing>
  <thing id="raFGImBloS" name="Blood Scent" description="In addition to normal scent, an imandwa can determine if two creatures are related to each other by scent. An imandwa gains a +8 bonus to track by scent when tracking a creature that has a relative the imandwa is closely familiar with. The imandwa can smell out the general location of relatives of a creature it is familiar with at a range of up to 100 miles, though once within a 5 - mile radius it must get within normal scent range to pinpoint such a creature&apos;s location." compset="RaceSpec" summary="You can determine if two creatures are related to each other by scent.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="SpecType" tag="Sense"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="Final" priority="20000"><![CDATA[#situational[hero.child[skSurvival],"+8 bonus to scent track a blood relative.",field[name].text]]]></eval>
    </thing>
  <thing id="raFGImShaF" name="Shadow Form" description="As a swift action an imandwa can become a piece of living shadow. It gains full concealment (50% miss chance) and becomes incorporeal. An imandwa&apos;s aura of disease is 50% likely to affect those within its area each round, even when incorporeal." compset="RaceSpec" summary="As a swift action you become incorproeal and gain full concealment.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGImUnhA" name="Unheard Alarm" description="If an imandwa&apos;s place of birth or death is threatened, or another imandwa senses an impending threat to them, the imandwa instinctively knows it. Most imandwa rush back to defend the places where they can regenerate." compset="RaceSpec" summary="You instinctively know when your place of birth or death is threatened.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGImCalH" name="Call Home" description="If a creature holds a handful of earth from the place an imandwa was born or the place it died, it can command the imandwa to come forth, even into bright sunlight. The imandwa must be within range to hear such a command, unless it is given through magic means (such as a {i}sending{/i} spell). The imandwa must heed the call for one hour from hearing it (moving toward the creature to the best of its ability, or staying within 60 feet of the creature once the imandwa arrives), and is then free to act normally. An imandwa need only obey the first such call it hears each day." compset="RaceSpec" summary="A creature holding a handful of earth from your brith place can command you.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGImRetH" name="Return Home" description="If reduced to 0 hit points in combat, an imandwa assumes {i}shadow form{/i} and attempts to escape. It must reach the place of its birth or the place of its death and rest there (in solid form) within 2 hours or be utterly destroyed. (It can normally travel up to 9 miles in 2 hours.) Additional damage dealt to an imandwa forced into shadow form has no effect, but if killed while at rest it is utterly destroyed. \n\nOnce at rest, the imandwa is helpless until sundown. (If forced into shadow form at night, once at rest it remains helpless throughout the night and all the next day, only regaining its power when the sun goes down again.) At sundown, the imandwa gains 5 hp and is no longer helpless." compset="RaceSpec" summary="If reduced to 0 hit points in combat, you assume {i}shadow form{/i} and attempt to escape.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGBruThr" name="Brutal Throw" description="Survival in the jungle requires brutal tactics. A cyclops can, as a full round action, throw his longspear even though it is not normally a valid ranged weapon. This special ranged attack has a range increment of 50 ft." compset="RaceSpec" summary="As a full round action throw a longspear with a range increment of 50 ft.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGImpSig" name="Impaired Sight" description="Due to its single eye, a cyclops has difficulty judging depth and suffers a -12 penalty on vision based Perception checks." compset="RaceSpec" summary="You have -12 penalty on vision based Perception checks.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Skill"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="Final" priority="20000"><![CDATA[#situational[hero.child[skPercep],"-12 on vision based checks",field[name].text]]]></eval>
    </thing>
  <thing id="raFGCyJuHu" name="Jungle Hunter" description="A cyclops is adroit at hunting in the jungles. It gains a +4 bonus on Stealth checks made in a jungle environment (in addition to its standard racial bonus). In addition, a cyclops makes use of the deadly poison dart frogs native to their area, coating the obsidian blades of their longspears in the deadly toxin. A cyclops never risks poisoning itself when applying this specific poison.\n\n{B}Dart Frog Venom{/b}\n{b}Type{/b} poison, injury or ingested; {b}Save{/b} Fortitude DC 19 \n{b}Frequency{/b} 1/round for 6 rounds \n{b}Effect{/b} 1d3 Con damage; {b}Cure{/b} 2 saves" compset="RaceSpec" summary="You use deadly poison dart frogs on your longspears.{BR}{b}Type{/b} poison, injury or ingested; {b}Save{/b} Fortitude DC 19{BR}{b}Frequency{/b} 1/round for 6 rounds{BR}{b}Effect{/b} 1d3 Con damage; {b}Cure{/b} 2 saves">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Skill"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="Final" priority="20000"><![CDATA[
    #situational[hero.child[skStealth],"+4 in jungle environments",field[name].text]
    
    ~ All manufactored weapon attacks
    foreach pick in hero from BaseWep where "!wFtrGroup.Natural"
       #extradamage[eachpick," plus poison",field[name].text]
    nexteach]]></eval>
    </thing>
  <thing id="raFGKarDeT" name="Death Throes" description="When slain, a karikanti thrashes about in a frenzy dealing 2d6 points of damage to any creature within five feet. A Reflex save halves this damage. The save DC is Constitution-based." compset="RaceSpec" summary="At death do 2d6 damage to all creatures within 5ft. A Reflex save halves the damage.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="StandardDC" tag="aCON"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGKarDrC" name="Drowning Curse" description="Once per day a karikanti may target any foe within 30 ft. with their drowning curse as a free action. The target must succeed on a Will save or take -10 to any Swim checks thereafter. A {i}remove curse{/i} or like enchantment cures this effect. The save DC is Charisma - based," compset="RaceSpec" summary="Curse a foe within 30ft to take -10 penalty on swim if they fail a Will save.">
    <fieldval field="trkMax" value="1"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="Day"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGKarMaE" name="Man Eater" description="A karikanti can spend 1 minute devouring a medium sized humanoid foe to regain 5 hit points, or gain 5 temporary hit points if they are already at full hit points. Temporary hit points gained in this manner persist for 1 hour. A karikanti cannot gain more than 30 temporary hit points using this ability." compset="RaceSpec" summary="Eat a medium humanoid to gain 5 HP or 5 Tempoary HP if at full hit points.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="AbilType" tag="Super"/>
