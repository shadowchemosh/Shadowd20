    <tag group="AbilType" tag="Extra"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGBCBurr" name="Burrow through Wood" description="Black Curdles can burrow through wood and creep in the dark spaces between bulkheads, eating through sturdy timbers as it does. Treat this as a burrow speed of 10 feet that only applies to wooden structures." compset="RaceSpec">
