  <thing id="xxFGSkMod" name="Racial Modifiers" compset="RaceSpec">
    <tag group="SpecType" tag="Skill"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="PostLevel" priority="20000"><![CDATA[
      var sSearch as string
      var sNames  as string
      var iX      as number
      var nCount  as number
      var Text    as string

      ~ total abValue and Value.? tags
      call TotalValue
      ~ get all the target tags
      sSearch = tagids[Target.?,"|"]
      ~ change to thingid tags instead
      sSearch = replace(sSearch,"Target.","thingid.",0)
      ~ count the number of targets so we can correctly format the text
      nCount = tagcount[Target.?]

      ~ Go through all skills based on the target tags
      foreach pick in hero from BaseSkill where sSearch
         iX += 1
         ~ give the racial bonus value
         eachpick.field[Bonus].value += field[abValue].value
         ~ if only one skill set name here
         if (nCount = 1) then
            sNames = eachpick.field[name].text
         ~..if more than one name then correct set commas and "and" wording here
         else
            if (iX = 1) then
               sNames = eachpick.field[name].text
            elseif (iX = nCount) then
               sNames &= " and " & eachpick.field[name].text
            elseif (iX > 1) then
               sNames &= ", " & eachpick.field[name].text
            endif
         endif
      nexteach
      ~ Change live name to the full value so we display correctly on a printed stat block
      field[livename].text = signed(field[abValue].value) & " racial bonus on " & sNames & "."]]></eval>
    </thing>
