    <tag group="Usage" tag="Day"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="ProductId" tag="FrogGod"/>
    </thing>
  <thing id="raFGQNcSpA" name="Spit Acid" description="Once every 1d4 rounds, a necroworm queen can spew forth a 30 ft. cone of acid which deals 4d6 points of damage to anyone in range (Reflex save DC 18 for half damage). The save DC is Dexterity-based." compset="RaceSpec">
    <tag group="StandardDC" tag="aDEX"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="AbilType" tag="Extra"/>
