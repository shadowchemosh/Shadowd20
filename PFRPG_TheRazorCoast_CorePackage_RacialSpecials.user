  <thing id="raFGPyroma" name="Pyromaniac" description="Members of this race are treated as 1 level higher when casting spells with the fire descriptor, using granted powers of the Fire domain, using bloodline powers of the fire elemental bloodline, using the revelations of the oracle&apos;s flame mystery, and when determining the damage of alchemist bombs that deal fire damage. This ability does not give members of this race early access to level-based powers; it only affects powers that they could already use without this ability. If a member of this race has a Charisma of 11 or higher, it also gains the following spell-like abilities: 1/day - {i}dancing lights{/i}, {i}flare{/i}, {i}prestidigitation{/i}, {i}produce flame{/i}. The caster level for these spell-like abilities is equal to individual&apos;s character level." compset="RaceSpec" summary="+1 level for effects of several flame based class abilities.">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="FrogGod"/>
    <bootstrap thing="raGnProdFl"></bootstrap>
    <bootstrap thing="raGnPresti"></bootstrap>
    <bootstrap thing="raGnFlare"></bootstrap>
    <bootstrap thing="raGnDance"></bootstrap>
    <eval phase="PreLevel" priority="10000"><![CDATA[~+1 Fire subschool spell Caster level
    hero.childfound[ssFire].field[schCastLev].value += 1
    
    ~+1 effective level for the earth domain's powers
    hero.childfound[dFirDart].field[xEffectLev].value += 1
    hero.childfound[dFirRes6].field[xEffectLev].value += 1
    hero.childfound[dFirRes12].field[xExtraLev].value += 1
    hero.childfound[dFirRes20].field[xExtraLev].value += 1
    
    ~+1 effective level for the ash domain's powers
    hero.childfound[dAshWall].field[xEffectLev].value += 1
    
    ~+1 effective level for the smoke domain's powers
    hero.childfound[dSmoCloud].field[xEffectLev].value += 1
    
    ~only apply the fire elemental bloodline's effects if we chose fire
    if (hero.picklives[cSorElem] <> 0) then
      if (hero.child[cSorElem].tagis[BloodEner.Fire] <> 0) then
        hero.childfound[cSorEleRay].field[xEffectLev].value += 1
        hero.childfound[cSorERFire].field[xEffectLev].value += 1
        hero.childfound[cSorEBlast].field[xEffectLev].value += 1
        hero.childfound[cSorEMFire].field[xEffectLev].value += 1
        hero.childfound[cSorElBody].field[xEffectLev].value += 1
        endif
      endif
    
    ~ Oracle's Mystery of Flames
    hero.childfound[cOraBurnMa].field[xEffectLev].value += 1
