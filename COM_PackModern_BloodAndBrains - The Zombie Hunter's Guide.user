    <exprreq message="Dexterity 13 required." iserror="no"><![CDATA[child[aDEX].field[aFinalVal].value >= 13]]></exprreq>
    </thing>
  <thing id="fMPBBCool" name="Cool" description="You are able to perform calmly under pressure.\n\nBenefit: You receive a +4 competence bonus to Concentration checks." compset="Feat" summary="+4 competence bonus to Concentration checks" uniqueness="useronce">
