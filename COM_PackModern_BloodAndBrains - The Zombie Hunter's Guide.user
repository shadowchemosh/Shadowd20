  <thing id="cMPBBChNE" name="Channel Negitive Energy" description="Channel negative energy to injure the living and heal the undead.\n\nYou can command undead Charisma modifier + 3 times per day." compset="CustomSpec" summary="You gain the ability to Channel Negitive Energy" uniqueness="unique">
    <fieldval field="shortname" value="Channel Negitive Energy"/>
    <usesource source="MPRPGOBB"/>
    <tag group="SpecSource" tag="arMPBBokor"/>
    <tag group="AlgnForbid" tag="Good"/>
    <tag group="ClassPanel" tag="Channel" name="Channeling Only" abbrev="Channeling Only"/>
    <tag group="HasAbility" tag="cAPaChanEn"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AbilType" tag="Super" name="Supernatural Ability" abbrev=" (Su)"/>
    <tag group="SpecSource" tag="arMPUnHlKn"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="Secondary" name="Secondary" abbrev="Secondary"/>
    <tag group="User" tag="Tracker"/>
    <tag group="ChargeCalc" tag="AttrUses3"/>
    <tag group="Usage" tag="Day"/>
    <tag group="ChargeAttr" tag="aCHA"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <bootstrap thing="xChannel">
      <autotag group="ClSpecWhen" tag="1"/>
      <autotag group="Helper" tag="ChannelNeg"/>
      <autotag group="Hero" tag="ChanDepend"/>
      <autotag group="SpecialTab" tag="SQClsAbil"/>
      </bootstrap>
    <eval phase="PostLevel" priority="1000">perform hero.assign[Hero.ChanNeg]

      ~tell the hero that if we take Extra Channeling, it needs to show an independant copy
      perform hero.assign[Hero.ChanNeg]

      hero.child[xChannel].field[abValue].value += field[xTotalLev].value</eval>
    <eval phase="Final" priority="10000" index="2"><![CDATA[   ~our bonus is level 
      var bonus as number
      bonus = field[xTotalLev].value / 2
      field[abValue].value += maximum(round(bonus,0,-1),1)
      field[livename].text = "Channel Negitive Energy - " & field[abValue].value & "d6"
      field[abSumm].text = field[abValue].value & "d6 of Negitive Energy."]]></eval>
    </thing>
