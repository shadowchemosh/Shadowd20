    <tag group="AbilType" tag="Extra"/>
    <tag group="Discipline" tag="BrokenBlad"/>
    <eval phase="PostAttr" priority="15000"><![CDATA[
      ~ Don't do anything if the stance isn't activated
      doneif (field[abilActive].value = 0)

      ~ Increase size for CMB and CMD
      Call PWszUpCMD
      Call PWszUpCMB

      ~ Add initiation bonus to CMB/CMD checks for grapple, bullrush, and overrun
      hero.childfound[manGrapple].field[Bonus].value += field[mIM].value
      hero.childfound[manOverrun].field[Bonus].value += field[mIM].value
      hero.childfound[manBullRus].field[Bonus].value += field[mIM].value]]></eval>
