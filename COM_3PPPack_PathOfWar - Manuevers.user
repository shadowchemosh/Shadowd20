    <tag group="mInitAct" tag="Swift1"/>
    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    </thing>
  <thing id="staPWGoGA6" name="Golden General&apos;s Attitude" description="A true Golden Lion disciple is a beacon of command on the field of battle, inspiring his troops by his mere presence. All allies within 60-ft. of the initiator gain a +1 morale bonus to attack and damage rolls, and to saves vs fear effects per four initiator levels." compset="PWManuever" summary="Stance - Grant a morale bonus to attack and damage rolls, and saves against fear effects to all allies within 60 ft. vs fear, +1 for every 4/initiator levels." uniqueness="useronce">
    <fieldval field="mPreReq" value="2"/>
    <usesource source="srcPathOfW"/>
    <tag group="User" tag="Activation"/>
    <tag group="Discipline" tag="GoldenLion"/>
    <tag group="PathOfWar" tag="Stance"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="mType" tag="Stance"/>
    <tag group="mLevel" tag="6"/>
    <tag group="mInitAct" tag="Swift1"/>
    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    <eval phase="PostAttr" priority="15000"><![CDATA[
      ~ Don't do anything if not active
      doneif (field[abilActive].value = 0)

      ~ Calculate our bonus of 1/4
      field[abValue].value = round(field[mIL].value/4,0,-1)

      ~ If we have Inspiring Banner and its active, add its bonus
      if (hero.tagis[Activated.cPW1InspBn] = 1) then
          field[abValue].value += #value[cPW1InspBn]
      endif

      ~ Attack and damage bonus
      #applybonus[BonMorale, hero.child[Attack], field[abValue].value]
      #applybonus[BonMorale, hero.child[Damage], field[abValue].value]

      ~ Bonus vs. fear
      #situational[hero.childfound[svWill],signed(field[abValue].value) & " morale vs. fear",field[thingname].text]]]></eval>
    </thing>
  <thing id="staPWGoLC4" name="Golden Lion Charger" description="A student of the art of war and of the Golden Lion, a disciple who has training in the art of engaging an enemy knows that sometimes a powerful opening charge can be the edge the warrior needs. While in this stance and performing a charge, the character gains an additional +2 bonus to charge attacks (making the bonus received from a charge attack a total of +4) and when charging through a threatened square, the character does not provoke attacks of opportunity." compset="PWManuever" summary="Stance: When charging, gain an additional +2 bonus to attack and never provoke attacks of opportunity for charging." uniqueness="useronce">
    <fieldval field="mPreReq" value="1"/>
    <fieldval field="abValue" value="2"/>
    <usesource source="srcPathOfW"/>
    <tag group="Discipline" tag="GoldenLion"/>
    <tag group="PathOfWar" tag="Stance"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="mType" tag="Stance"/>
