    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PostAttr" priority="15000"><![CDATA[
      ~ If we're not enabled, get out now
      doneif (field[abilActive].value = 0)
      ~ Stop if we're not dual-wielding or using a double weapon
      doneif (hero.tagcount[Hero.EquipWep] + hero.tagcount[Hero.EquipDbl] <> 2)

      ~ Apply AC penalty
      hero.childfound[ArmorClass].field[Penalty].value += field[abValue].value

      ~ Add extra damage
      foreach pick in hero from BaseWep where "Hero.MainHand | Hero.OffHand"
        eachpick.field[wDamBonus].value += field[mIM].value
        #extradamage[eachpick, signed(field[abValue2].value) & "d" & field[abValue3].value, field[thingname].text]
      nexteach]]></eval>
    </thing>
  <thing id="staPWCiSt1" name="Circular Stance" description="Practitioners of Scarlet Throne know that to be a lord amongst warriors, he must walk the path of battle alone, and they understand that often times that may be beset by all sides. By adopting the Circular Stance, the disciple learns to defend himself from the predations of those who would think him outflanked. While in this stance, the character is never considered flanked (this thwarts sneak attack attempts as well, provided that the rogue or ninja&apos;s level does not exceed the disciple&apos;s initiator level by 4 or more)." compset="PWManuever" summary="Stance: Flankers do not receive combat bonus against you when flanking." uniqueness="useronce">
    <fieldval field="mPreReq" value="0"/>
    <usesource source="srcPathOfW"/>
    <tag group="User" tag="Activation"/>
    <tag group="Discipline" tag="ScarletThr"/>
    <tag group="PathOfWar" tag="Stance"/>
    <tag group="AbilType" tag="Extra"/>
