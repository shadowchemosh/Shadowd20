    <tag group="mType" tag="Stance" name="Stance" abbrev="Stance"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <tag group="Discipline" tag="SteelSerp"/>
    <eval phase="PostAttr" priority="15000" index="2"><![CDATA[
      ~ Don't do anything if the stance isn't activated
      doneif (field[abilActive].value = 0)

      ~ Add +4 natural armor bonus
      #applybonus[tACNatural, hero.child[ArmorClass], field[abValue].value]]]></eval>
    </thing>
  <thing id="staPWHdKil" name="Hooded Killer&apos;s Stance" description="The disciple adopts the mannerisms and cold blooded nature of the hooded cobra, the lord of serpent kind. While in this stance, the disciple gains an additional +3d6 sneak attack dice (see rogue class feature) and a competence bonus to Intimidate and Stealth checks equal to half of his initiator level. If the disciple possessed the sneak attack class feature previously, the benefits of this stance stack with his class feature." compset="PWManuever" summary="Disciple gains +3d6 sneak attack dice and half the character’s initiator level to Intimidate and Stealth checks." uniqueness="useronce">
    <fieldval field="abValue" value="3"/>
    <fieldval field="mPreReq" value="2"/>
    <usesource source="srcPathOfW" parent="pDscar" name="Path of War v1.2a"/>
    <tag group="mDuration" tag="Stance" name="Stance" abbrev="Stance"/>
    <tag group="mInitAct" tag="Swift1" name="1 swift action" abbrev="1 swift action"/>
    <tag group="mLevel" tag="5"/>
    <tag group="mRange" tag="Personal" name="Personal" abbrev="Personal"/>
    <tag group="mTarget" tag="You" name="You" abbrev="You"/>
    <tag group="mType" tag="Stance" name="Stance" abbrev="Stance"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <tag group="Discipline" tag="SteelSerp"/>
    <tag group="PathOfWar" tag="Stance" name="Stance" abbrev="Stance"/>
    <bootstrap thing="raSneakAtt">
      <containerreq phase="First" priority="500"><![CDATA[fieldval:abilActive <> 0]]></containerreq>
      </bootstrap>
    <eval phase="PostAttr" priority="15000" index="2"><![CDATA[
      ~ Don't do anything if the stance isn't activated
      doneif (field[abilActive].value = 0)

      ~ Give the +3d6 sneak attack damage bonus to the Racial Special because if 
      ~ the rogue/ninja class ability exists the Racial Special adds to it for us.
      #value[raSneakAtt] += field[abValue].value
      perform hero.child[raSneakAtt].delete[Helper.SpcDisable]

      ~ Get IL and divide by 2, add result to Intim and Stealth as comp.bonus
      field[abValue2].value = round(field[mIL].value/2,0,-1)
      #applybonus[BonComp, hero.child[skIntim], field[abValue2].value]
      #applybonus[BonComp, hero.child[skStealth], field[abValue2].value]]]></eval>
    </thing>
  <thing id="staPWSptCb" name="Spitting Cobra Stance" description="Some serpents spit their venom, and as they are deadly at a distance so does the disciple learn to be deadlier at range with his attacks. When at range, thrown weapons inflict an additional 2d6 points of damage and throwing them does not provoke attacks of opportunity. Use of poison (mundane or magical poisons such as stings or prana maneuvers) are more vicious as well, adding +1 to each damage die or ability damage die rolled for its effect (effects with a flat number of damage increase this by +1)." compset="PWManuever" summary="Stance - Increases damage done by thrown weapons and poison duration." uniqueness="useronce">
    <fieldval field="abValue" value="2"/>
    <fieldval field="mPreReq" value="2"/>
    <usesource source="srcPathOfW" parent="pDscar" name="Path of War v1.2a"/>
