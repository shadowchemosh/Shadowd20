    <tag group="PathOfWar" tag="Manuever"/>
    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    </thing>
  <thing id="manPWFlStr" name="Flurry Strike" description="The disciple of the Broken Blade learns to maximize openings in his opponent&apos;s defenses and makes lightning fast attacks whenever possible. As a standard action, the initiator may make two attacks at his full base attack bonus." compset="PWManuever" summary="Make two quick attacks on a target enemy." uniqueness="useronce">
