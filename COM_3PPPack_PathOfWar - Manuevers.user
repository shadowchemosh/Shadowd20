    <tag group="mInitAct" tag="Swift1"/>
    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    <bootstrap thing="pPWTriLion">
      <containerreq phase="First" priority="500"><![CDATA[fieldval:abilActive <> 0]]></containerreq>
      </bootstrap>
    </thing>
  <thing id="staPWTrSS6" name="True Shot Stance" description="The Solar Wind disciple has honed his incredible senses and while in this stance uses his inner vision to strike his foes with his most deadly attacks. While in this stance, the initiator ignores a foe&apos;s partial concealment or any cover and he doubles the critical threat range on any ranged weapon he uses. Other effects that increase critical threat range, such as a {i}keen{/i} weapon, do not stack with this effect." compset="PWManuever" summary="Stance - Ignore concealment, double the threat range on ranged attacks." uniqueness="useronce">
    <fieldval field="mPreReq" value="2"/>
    <usesource source="srcPathOfW"/>
    <tag group="User" tag="Activation"/>
    <tag group="Discipline" tag="SolarWind"/>
    <tag group="PathOfWar" tag="Stance"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="mType" tag="Stance"/>
    <tag group="mLevel" tag="6"/>
    <tag group="mInitAct" tag="Swift1"/>
    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    <eval phase="PreLevel" priority="6000"><![CDATA[
      ~ Stop now if stance not activated
      doneif (field[abilActive].value = 0)

      ~ Improve crit range for ranged weapons
      ~ We use Broadcast.ImpCrit because it's not supposed to stack with other effects that increase crit range
      foreach pick in hero from BaseWep where "(wFtrGroup.Bows | wFtrGroup.Crossbows | wFtrGroup.Thrown) & !Broadcast.ImpCrit"
        perform eachpick.assign[Broadcast.ImpCrit]
      nexteach]]></eval>
    </thing>
  <thing id="staPWUnMo3" name="Unfettered Movement" description="The Scarlet Throne disciple is swift in battle and in his ability to go from one skirmish to the next to choose his next foe. By assuming this stance, the disciple gains a +10-ft. bonus to his base speed and gains a +4 dodge bonus to his armor class against attacks of opportunity." compset="PWManuever" summary="Stance - Gain an additional +10 ft. to base speed, +4 bonus to AC vs. attacks of opportunity." uniqueness="useronce">
    <fieldval field="mPreReq" value="1"/>
    <fieldval field="abValue" value="10"/>
    <fieldval field="abValue2" value="4"/>
    <usesource source="srcPathOfW"/>
    <tag group="Discipline" tag="ScarletThr"/>
    <tag group="PathOfWar" tag="Stance"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="mType" tag="Stance"/>
    <tag group="mLevel" tag="3"/>
    <tag group="mInitAct" tag="Swift1"/>
    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PostAttr" priority="15000"><![CDATA[
      ~ Don't do anything if the stance isn't activated
      doneif (field[abilActive].value = 0)
      ~ Stop if we're using a shield (except bucklers)
      ~ This rule has been disabled as per new ruling from DSP
      ~if (hero.tagis[Hero.EquipShld] <> 0) then
      ~  doneif (hero.tagis[ShldClass.Buckler] = 0)
      ~endif

      ~ Add to base speed
      hero.child[Speed].field[tSpeed].value += field[abValue].value

      ~ Add situational AC bonus
      #situational[hero.childfound[ArmorClass],signed(field[abValue2].value) & " dodge vs Attacks of Opportunity",field[thingname].text]]]></eval>
    </thing>
  <thing id="manPWDmSt1" name="Dimensional Strike" description="By shifting slightly between two worlds, the disciple confuses the senses of his foe so he may land his attack more easily. The initiator initiating this strike causes his foe to be flat-footed to this attack." compset="PWManuever" summary="For a brief moment, the disciple becomes difficult to perceive; resolving the attack against the target as if they were flat-footed." uniqueness="useronce">
