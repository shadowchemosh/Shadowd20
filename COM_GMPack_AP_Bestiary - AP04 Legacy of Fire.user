    <eval phase="Final" priority="10000" index="3"><![CDATA[~ remove all movement except for Fly

hero.childfound[Speed].field[tSpeed].value = 0
hero.childfound[Speed].field[tSpeedMod].value = 0

foreach pick in hero from Ability where "SpecType.Movement & !thingid.xFly"
  eachpick.field[abValue].value = 0
nexteach

hero.childfound[xFly].field[abValue].value = round(.5 * hero.childfound[xFly].field[abValue].value,0,-1)

perform hero.childfound[xFly].delete[Maneuver.?]
perform hero.childfound[xFly].assign[Maneuver.Perfect]


perform hero.delete[RaceSpells.?]]]></eval>
    <eval phase="First" index="4"><![CDATA[~ hide weapons 

foreach pick in hero from BaseNatWep where "!thingid.wBite"
  perform eachpick.assign[Hide.Weapon]
nexteach

perform hero.childfound[raFrightPr].assign[Helper.SpcDisable]
perform hero.childfound[raFrightPr].assign[Hide.Special]]]></eval>
    <eval phase="First"><![CDATA[~ adjust size

if (herofield[tSize].value >= 4) then
