   hero.child[aSTR].field[aStartMod].value -= 12
   hero.child[aDEX].field[aStartMod].value -= -4
   hero.child[ArmorClass].field[tACNatural].value -= 2
elseif (herofield[tSize].value = 0) then
   herofield[tSize].value = -2
   hero.child[aSTR].field[aStartMod].value -= 8
   hero.child[aDEX].field[aStartMod].value -= -4
elseif (herofield[tSize].value = -1) then
   herofield[tSize].value = -3
   hero.child[aSTR].field[aStartMod].value -= 6
   hero.child[aDEX].field[aStartMod].value -= -4
elseif (herofield[tSize].value = -2) then
   herofield[tSize].value = -4
   hero.child[aSTR].field[aStartMod].value -= 2
   hero.child[aDEX].field[aStartMod].value -= -4
elseif (herofield[tSize].value = -3) then
   herofield[tSize].value = -4
   hero.child[aDEX].field[aStartMod].value -= -2
endif]]></eval>
    <eval phase="UserFinal" index="5"><![CDATA[~ spells

~perform hero.delete[Hero.Caster]
~perform hero.delete[Hero.Arcane]

perform hero.delete[Hero.ClsOpLevUp]

foreach pick in hero from BaseSpell where "!Helper.SpellLike"
 perform eachpick.assign[Hide.Spell]
nexteach]]></eval>
    <eval phase="Final" priority="10000" index="2"><![CDATA[~ apply DR/bludgeoning

#applydr[xDamRdB,hero.childfound[xDamRdMag].tagvalue[Value.?]]

~ CR

~ handles HL standard dragons and variant age catagory dragons

if (hero.tagis[DragonAge.?] <> 0) then

var ageCat as number
ageCat = hero.childfound[raxDragonA].tagvalue[Value.?]

if (ageCat >= 11) then
  herofield[tCR].value -= 5
elseif (ageCat >= 9) then
  herofield[tCR].value -= 4
elseif (ageCat >= 7) then
  herofield[tCR].value -= 3
elseif (ageCat >= 5) then
  herofield[tCR].value -= 2
endif

else

if (hero.tagcount[Race.rYAdu?] + hero.tagcount[Race.rAdu?] <> 0) then
   herofield[tCR].value -= 2
elseif (hero.tagcount[Race.rMAdu?] + hero.tagcount[Race.rOld?] <> 0) then 
   herofield[tCR].value -= 3
elseif (hero.tagcount[Race.rVOld?] + hero.tagcount[Race.rAnc?] <> 0) then
   herofield[tCR].value -= 4
elseif  (hero.tagcount[Race.rWyr?] + hero.tagcount[Race.rGWyr?] <> 0) then
   herofield[tCR].value -= 5
endif

endif]]></eval>
