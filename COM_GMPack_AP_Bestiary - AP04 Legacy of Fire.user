    <fieldval field="rHPStart" value="22"/>
    <fieldval field="rSTR" value="13"/>
    <fieldval field="rCON" value="9"/>
    <fieldval field="rINT" value="-6"/>
    <fieldval field="rCHA" value="-2"/>
    <fieldval field="rSpeed" value="40"/>
    <fieldval field="rFeat" value="-3"/>
