  <thing id="mFGArmCoat" name="Armored coat" description="This leather coat is often worn over naval dress armor by officers, along with pirate captains and others seeking more protection from weapons but wish to quickly escape the armor if washed overboard.\n\nThe coat can be donned or removed with a move action. If worn over other armor, use the better AC bonus and worse value in all other categories. An armored coat has no effect when worn with heavy armor. If wearing magical armor and a magical armored coat, only the coat&apos;s magic effects apply until removed." compset="Armor">
    <fieldval field="gWeight" value="20"/>
    <fieldval field="gSizeCost" value="50"/>
    <usesource source="CampRazorC"/>
    <tag group="ArmorCheck" tag="2" name="2" abbrev="2"/>
    <tag group="ArmorClass" tag="Medium" name="Medium Armor" abbrev="Medium"/>
    <tag group="ArmorMaxDx" tag="3" name="3" abbrev="3"/>
    <tag group="EquipType" tag="Metal" name="Metal" abbrev="Metal"/>
    <tag group="ArmorAC" tag="4" name="4" abbrev="4"/>
    <tag group="ArmorArcFl" tag="20" name="20" abbrev="20"/>
    <tag group="ArmorCateg" tag="Slows" name="Slows" abbrev="Slows"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="PreLevel" priority="10050"><![CDATA[
      ~ this script is for regular armor
      doneif (container.ishero = 0)

      ~ if we're equipped
      if (field[gIsEquip].value <> 0) then
        ~ if the hero is wearing 2 suits of armor
        if (hero.tagcount[Hero.EquipArmor] = 2) then
          ~ if that one's not heavy
          if (hero.tagis[ArmorClass.Heavy] = 0) then
            ~ delete all the Hero.EquipArmor tags
            perform hero.delete[Hero.EquipArmor]
            ~ then assign only one
            perform hero.assign[Hero.EquipArmor]
            
            ~ magic armor handles it slightly differently
            perform hero.delete[Hero.EqpArmor]
            perform hero.assign[Hero.EqpArmor]

            ~ if we're worn with other armor, our ACP and Arcane Failure %
            ~ don't stack with those of other armors
            ~ so add a special tag that that tells HL to handle it that way
            perform assign[Helper.NoStackACP]
            endif
          endif
        endif]]></eval>
    <eval phase="PreLevel" priority="10050" index="2"><![CDATA[
      ~ this script is for custom/magic armor
      doneif (container.ishero <> 0)
      
      ~ if we're equipped
      if (container.parent.field[gIsEquip].value <> 0) then
        ~ if the hero is wearing 2 suits of armor
        if (hero.tagcount[Hero.EquipArmor] = 2) then
          ~ if that one's not heavy
          if (hero.tagis[ArmorClass.Heavy] = 0) then
            ~ delete all the Hero.EquipArmor tags
            perform hero.delete[Hero.EquipArmor]
            ~ then assign only one
            perform hero.assign[Hero.EquipArmor]
            
            ~ magic armor handles it slightly differently
            perform hero.delete[Hero.EqpArmor]
            perform hero.assign[Hero.EqpArmor]
            
            ~ if we're worn with other armor, our ACP and Arcane Failure %
            ~ don't stack with those of other armors
            ~ so add a special tag that that tells HL to handle it that way
            perform container.parent.assign[Helper.NoStackACP]
            endif
          endif
        endif]]></eval>
    </thing>
