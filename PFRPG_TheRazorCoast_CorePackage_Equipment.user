    <tag group="Helper" tag="AlwaysAvl"/>
    <tag group="User" tag="Activation"/>
    <tag group="User" tag="Tracker"/>
    <tag group="gType" tag="Tattoo" name="Magical Tattoo" abbrev="Tattoo"/>
    <tag group="Usage" tag="MinuteDay" name=" minute/day"/>
    <tag group="ProductId" tag="FrogGod"/>
    <eval phase="First">~ Pull the Slot tag from what was chosen and change it to be a Hero
~ tag instead of an ExtraSlot tag.
perform field[usrChosen1].chosen.pulltags[ExtraSlot.?,Hero]
~ Push the Equipped magic item tag to the hero as we are taking up 
~ that slot now as a magic tattoo.
perform forward[Hero.Eqp?]</eval>
    <eval phase="PostLevel" priority="10000" index="2"><![CDATA[
~ If not active get out now
doneif (field[abilActive].value = 0)

~ Give a +1 luck bonus to H/D to all melee weapons
foreach pick in hero from BaseWep where "wCategory.Melee"
  #applybonus[BonLuck,eachpick,1]
nexteach]]></eval>
    <prereq message="Too many magic items equipped in this slot.">
      <validate><![CDATA[
~ If we are not a pick yet then we are valid.
validif (@ispick <> 1)
var sEquip as string
var sExtra as string
var nSlots as number
~ Default number of magic items per slot is 1
nSlots = 1
~ get a string of the equipment slot tag
sEquip = altpick.tagids[Hero.Eqp?]
~ make an extra slot tag out of the equipped tag
sExtra = replace(sEquip,"Hero.","ExtraSlot.",0)
~ increase the number of magic items allowed in a specific slot
~ by the number of extra tags on the hero.
nSlots += hero.tagcountstr[sExtra]
~ if we are the ring slot then increase the number allowed by additional 1
If (compare(sEquip,"Hero.EqpRing") = 0) Then
   nSlots += 1
Endif

~ If we have more magic items in this slot then allowed then
~ valid invalid and set the message to the gamer.
If (hero.tagcountstr[sEquip] > nSlots) Then
   @valid = 0
   @message = "You may only have " & nSlots & " " & tagnames[Hero.Eqp?] & " items equipped at one time."
Else
   @valid = 1
Endif]]></validate>
      </prereq>
    </thing>
  <thing id="ioFGTatBlB" name="Blue Bird Tattoo" description="The blue bird, sparrow, or swallow depicted within this tattoo represents the 5000 miles the bearer has sailed. This tattoo grants a +2 competence bonus on Diplomacy, Intimidate, and Sense Motive checks against sailors and pirates. \n\nCONSTRUCTION\n{b}Requirements{/b} Inscribe Magical Tattoo, {i}hypnotism{/i}; {b}Cost{/b} 450 gp" compset="Wondrous" holdable="no">
