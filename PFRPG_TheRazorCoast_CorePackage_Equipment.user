  <thing id="ioFGTatBlK" name="Blade&apos;s Kiss Tattoo" description="A tattoo depicting a dagger, knife, or other melee weapon favored usually by the bearer grants a +1 luck bonus on attack and weapon damage rolls for melee weapons for 1 minute per day. \n\nCONSTRUCTION\n{b}Requirements{/b} Inscribe Magical Tattoo, {i}divine favor{/i}; Cost 360 gp" compset="Wondrous" holdable="no">
    <fieldval field="gCost" value="720"/>
    <fieldval field="iCL" value="1"/>
    <fieldval field="trkMax" value="1"/>
    <fieldval field="usrCandid1" value="component.ItemSlot &amp; !thingid.isEqpArmor &amp; !thingid.isEqpEyes &amp; !thingid.isEqpHBand"/>
    <fieldval field="actName" value="Luck Bonus"/>
    <usesource source="CampRazorC"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="iSchool" tag="Evocation"/>
