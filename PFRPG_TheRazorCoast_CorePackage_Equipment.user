  <thing id="abFGTeeDaj" name="Teeth of Dajobas" compset="Ability" uniqueness="unique">
    <comment>This is a helper object to place single things as we have two daggers.</comment>
    <usesource source="CampRazorC"/>
    <tag group="ProductId" tag="FrogGod"/>
    <bootstrap thing="spBladBar6">
      <autotag group="Helper" tag="ItemSpell"/>
      <autotag group="Usage" tag="Day"/>
      <autotag group="iFGTeeDajo" tag="ItemSpell"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <eval phase="First" priority="210" index="2"><![CDATA[
~ Count class levels
field[abValue2].value += hero.tagcount[Classes.?]
~ Add in the additional hit dice from the Fel Lycanthrope Template
field[abValue2].value += hero.tagcount[FelLycan.HitDice]

~ If 12+ assign level12 tag
If (field[abValue2].value >= 12) Then
   perform hero.assign[iFGTeeDajo.Level12]
Endif
~ If 15+ assign level15 tag
If (field[abValue2].value >= 15) Then
   perform hero.assign[iFGTeeDajo.Level15]
Endif
~ If 20+ assign level20 tag
If (field[abValue2].value >= 20) Then
   perform hero.assign[iFGTeeDajo.Level20]
Endif]]></eval>
    <eval phase="Final" priority="50000"><![CDATA[~ Set Caster Level
field[abValue].value += 12

~ Loop through all spells associated to the Teeth Artificat
foreach pick in hero from BaseSpell where "iFGTeeDajo.ItemSpell"
   ~ If we are bestow curse then make DC 12 + Wis
   If (eachpick.tagis[thingid.spBestCur3] <> 0) Then
      eachpick.field[sDC].value = 10 + eachpick.tagvalue[sLevel.?] + #attrmod[aWIS] 
      ~ The text says 12 so we need to adjust down
      eachpick.field[sDC].value += -1
      eachpick.field[sCL].value += field[abValue].value
   Endif
   ~ If we are Blade Barrier
   If (eachpick.tagis[thingid.spBladBar6] <> 0) Then
      ~ If we are a Lycanthrope
      If (hero.tagis[Ability.xLyChange] <> 0) Then
         ~ If we have both weapons equipped then give
         ~ blade barrier its DC and make it active
         If (hero.tagcount[iFGTeeDajo.Equipped] >= 2) Then
            eachpick.field[sDC].value = 10 + eachpick.tagvalue[sLevel.?] + #attrmod[aWIS] 
            eachpick.field[sCL].value += field[abValue].value
            perform eachpick.delete[Hide.Activation]
            perform eachpick.delete[Helper.SpcDisable]
            perform eachpick.delete[Helper.SpcNotWorn]
         Endif
      Endif
  Endif
nexteach]]></eval>
    <eval phase="Final" priority="9999" index="3"><![CDATA[
~ If we are not in hybrid or animal form get out now
doneif (hero.tagis[Hero.LyHybrid] + hero.tagis[Hero.LyAnimal] = 0)

~ At level 12 increase DR by 5
If (hero.tagis[iFGTeeDajo.Level12] <> 0) Then
   #value[xDamRdSil] += 5
Endif
~ At level 15 give cold resistance 10
If (hero.tagis[iFGTeeDajo.Level15] <> 0) Then
   #value[xDamRsCold] += 10
Endif
~ At level 20 also give DR good 
If (hero.tagis[iFGTeeDajo.Level20] <> 0) Then
   #value[xDamRdGood] += #value[xDamRdSil]
Endif]]></eval>
    </thing>
