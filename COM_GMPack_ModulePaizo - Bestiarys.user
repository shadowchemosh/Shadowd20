    <eval phase="PreLevel" priority="5000"><![CDATA[~ If we haven't chosen anything, get out now
      doneif (field[usrChosen1].ischosen  = 0)

var engy as string

if (field[usrChosen1].chosen.tagis[BloodEner.Acid] <> 0) then
  perform hero.child[xImmAcid].delete[Helper.SpcDisable]
  perform hero.child[xImmAcid].delete[Hide.Special]
  engy = "acid"
elseif (field[usrChosen1].chosen.tagis[BloodEner.Cold] <> 0) then
  perform hero.child[xImmCold].delete[Helper.SpcDisable]
  perform hero.child[xImmCold].delete[Hide.Special]
  engy = "cold"
elseif (field[usrChosen1].chosen.tagis[BloodEner.Elec] <> 0) then
  perform hero.child[xImmElec].delete[Helper.SpcDisable]
  perform hero.child[xImmElec].delete[Hide.Special]
  engy = "electricity" 
elseif (field[usrChosen1].chosen.tagis[BloodEner.Fire] <> 0) then
  perform hero.child[xImmFire].delete[Helper.SpcDisable]
  perform hero.child[xImmFire].delete[Hide.Special]
  engy = "fire"
endif

var nhd as number
nhd = hero.tagcount[Hero.HitDice]

hero.child[raBreath].field[abSumm].text = "Breath Weapon (80' line, " & nhd & "d8 " & engy & ", 1/1d4 rds., Reflex half)"]]></eval>
    </thing>
