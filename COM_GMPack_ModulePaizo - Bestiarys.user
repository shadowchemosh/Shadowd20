    <eval phase="First" index="4"><![CDATA[~ if we have a natural attack assign custom tag so we do not get an additonal slam attack

var cnt as number
cnt=0
foreach pick in hero where "wGroup.Natural & !(thingid.wConstrict | thingid.wUnarmed)"
  cnt += 1
nexteach

if (cnt > 1) then
 perform hero.assign[Custom.HasNatural]
endif]]></eval>
    <eval phase="PreLevel" priority="10000" index="5"><![CDATA[foreach pick in hero from BaseNatWep where "!(thingid.wConstrict | thingid.wUnarmed) & Helper.NatPrimary"
  #extradamage[eachpick," plus grab",field[name].text]
nexteach]]></eval>
    <eval phase="PreLevel" priority="10000" index="2"><![CDATA[var hd as number
hd = hero.tagcount[Hero.HitDice] - hero.tagcount[Classes.?]

~ assign CR based on HD
var cr as number
if (hd <= 1) then
   cr = 1
elseif (hd <= 2) then
   cr = 2
elseif (hd <= 4) then
   cr = 3
elseif (hd <= 7) then
   cr = 4
elseif (hd <= 11) then
   cr = 5
elseif (hd <= 15) then
   cr = 6
elseif (hd <= 19) then
   cr = 7
else
   cr = 8
endif
   herofield[tCR].value = cr

~ assign DR based on HD
var dr as number
if (hd <= 7) then
   dr = 5
elseif (hd <= 19) then
   dr = 10
else
   dr = 15
endif
  hero.child[xDamRd].field[abValue].value = dr

~ Force our Charisma to 1
  hero.child[aCHA].field[aNormForce].value = 1]]></eval>
    <eval phase="Final" index="6"><![CDATA[foreach pick in hero where "thingid.wConstrict & !Custom.PaintedC"
  perform eachpick.assign[Hide.Weapon]
nexteach]]></eval>
    <eval phase="First"><![CDATA[      ~ Many races and classes bootstrap feats, so go through them disabling them all
      foreach pick in hero from BaseFeat where "!Helper.ProfGroup"
        perform eachpick.assign[Helper.FtDisable]
        perform eachpick.assign[Hide.Special]
        nexteach 

~ Disable all special abilities apart from the ones that we bootstrap
      var searchexpr as string
      searchexpr = "!component.BaseFeat & !(SpecSource.tmuPainted) & !SpecType.Movement & !thingid.xConstruct"
        foreach pick in hero from Ability where searchexpr
          perform eachpick.assign[Helper.SpcDisable]
          perform eachpick.assign[Hide.Special]
          nexteach]]></eval>
    <eval phase="First" priority="5000" index="3"><![CDATA[~we don't have a INT score
      perform hero.child[aINT].assign[Helper.NoScore]]]></eval>
    </thing>
