      <autotag group="wMain" tag="1d4_4"/>
      <autotag group="Value" tag="1"/>
      <autotag group="Helper" tag="NatOverSec"/>
      </bootstrap>
    <bootstrap thing="raFCOpPoSe"></bootstrap>
    <bootstrap thing="spCharPer1">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Usage" tag="Day"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <eval phase="Final" priority="10000" index="2"><![CDATA[
      ~ Assign the Poison text to our Bite
      foreach pick in hero from BaseWep where "IsWeapon.wBite"
        #extradamage[eachpick," plus Poison",field[livename].text]
      nexteach]]></eval>
    <eval phase="PreLevel" priority="4000">#skillinnate[skClimb] += 4
      #skillinnate[skEscape] += 4
      #skillinnate[skPercep] += 4
      #skillinnate[skStealth] += 4</eval>
    <eval phase="First" priority="20000" index="3"><![CDATA[
      ~ If we've been disabled, get out now!
      doneif (tagis[Helper.SpcDisable] <> 0)

      ~ We need to be proficient with these weapons
      foreach thing in BaseWep where "wFtrGroup.Flails"
         perform eachthing.pulltags[WepProf.?]
      nexteach

      ~ Push all the Wepon Proficiency tags to the hero.
      ~ This will make our character proficient with all flails.
      ~ Note: This means the script should run as early as possible.
      perform hero.pushtags[WepProf.?]

      ~ If we are an exotic weapon assign helper tag to make us 
      ~ proficient
      var searchexpr as string
      searchexpr = tagids[WepProf.?,"|"]

      foreach pick in hero from BaseWep where searchexpr
          perform eachpick.assign[Helper.ExoticProf]
      nexteach]]></eval>
    </thing>
  <thing id="raFCTraSpr" name="Tranquilizing Spray" description="The scavenger worm can spit a viscous liquid in a 20-ft. cone. \n\nAll living creatures in the area must succeed on a DC 14 Fortitude save or fall asleep for 1d8+1 rounds. \nVictims can be awakened early with a DC 15 Heal check, as a full-round action, or with any variant of restoration or neutralize poison.\n\nOnce it attacks with its spray, the scavenger worm cannot do so again for 1d4 rounds.\n\nThe save DC is Constitution-based." compset="RaceSpec">
