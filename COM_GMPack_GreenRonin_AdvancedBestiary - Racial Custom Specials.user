  <thing id="rcuDPBabau" name="Demon, Babau" description="You are possessed by a babau demon." compset="RaceCustom" uniqueness="useronce">
    <usesource source="srcAdvBest"/>
    <tag group="AllowRCust" tag="cfgDmnPoss"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <bootstrap thing="raSneakAtt">
      <autotag group="SpecSource" tag="tmuDmnPoss"/>
      <assignval field="abValue" value="1"/>
      </bootstrap>
    <bootstrap thing="rauDPBabSl"></bootstrap>
    <eval phase="UserPreLv">#applybonus[BonProfane,hero.child[skStealth],4]
  #applybonus[BonProfane,hero.child[skPercep],4]

  herofield[tLevelAdj].value += 3
  herofield[tCR].value += 1
  hero.child[aSTR].field[aStartMod].value += 4
  hero.child[aDEX].field[aStartMod].value += 0
  hero.child[aCON].field[aStartMod].value += 4
  hero.child[aINT].field[aStartMod].value += 0
  hero.child[aWIS].field[aStartMod].value += 0
  hero.child[aCHA].field[aStartMod].value += 2

hero.child[aSTR].field[aNormMin].value = 1
hero.child[aDEX].field[aNormMin].value = 1
hero.child[aCON].field[aNormMin].value = 1
hero.child[aINT].field[aNormMin].value = 1
hero.child[aWIS].field[aNormMin].value = 1
hero.child[aCHA].field[aNormMin].value = 1</eval>
    <eval phase="First" priority="600" index="2"><![CDATA[hero.child[tmuDmnPoss].field[livename].text = "Babau-Possessed"]]>
      <before name="Template Name Modification"/>
      </eval>
    </thing>
