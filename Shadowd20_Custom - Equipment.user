    <eval phase="Attr" priority="200000" index="2"><![CDATA[
      perform hero.child[wUnarmed].setfocus
      doneif (state.isfocus = 0)
         
      ~ We need to pull all the bonuses
      #applybonus[BonEnhance,this,focus.field[BonEnhance].value]
      #applybonus[BonAlch,this,focus.field[BonAlch].value]
      #applybonus[BonComp,this,focus.field[BonComp].value]
      #applybonus[BonInsight,this,focus.field[BonInsight].value]
      #applybonus[BonLuck,this,focus.field[BonLuck].value]
      #applybonus[BonMorale,this,focus.field[BonMorale].value]
      #applybonus[BonProfane,this,focus.field[BonProfane].value]
      #applybonus[BonSacred,this,focus.field[BonSacred].value]
      #applybonus[BonTrait,this,focus.field[BonTrait].value]
      #applybonus[Bonus,this,focus.field[Bonus].value]
      ~ Specific melee bonuses
      #applybonus[atmBonAlch,this,focus.field[atmBonAlch].value]
      #applybonus[atmBonComp,this,focus.field[atmBonComp].value]
      #applybonus[atmBonEnh,this,focus.field[atmBonEnh].value]
      #applybonus[atmBonIns,this,focus.field[atmBonIns].value]
      #applybonus[atmBonLuck,this,focus.field[atmBonLuck].value]
      #applybonus[atmBonMora,this,focus.field[atmBonMora].value]
      #applybonus[atmBonProf,this,focus.field[atmBonProf].value]
      #applybonus[atmBonSacr,this,focus.field[atmBonSacr].value]
      #applybonus[atmBonTrt,this,focus.field[atmBonTrt].value]
      #applybonus[atmBonus,this,focus.field[atmBonus].value]
      #applybonus[atmModCirc,this,focus.field[atmModCirc].value]
      #applybonus[atmRacial,this,focus.field[atmRacial].value]
      ~ Specific melee penalties
      #applypenalty[atmPenComp,this,focus.field[atmPenComp].value]
      #applypenalty[atmPenLuck,this,focus.field[atmPenLuck].value]
      #applypenalty[atmPenMora,this,focus.field[atmPenMora].value]
      #applypenalty[atmPenProf,this,focus.field[atmPenProf].value]
      #applypenalty[atmPenSacr,this,focus.field[atmPenSacr].value]
      #applypenalty[atmPenTrt,this,focus.field[atmPenTrt].value]
      #applypenalty[atmPenalty,this,focus.field[atmPenalty].value]

      ~ If we're a custom/magic weapon, we need to alter our parent too
      if (container.ishero = 0) then
        ~ We need to pull all the bonuses
        #applybonus[BonEnhance,container.parent,focus.field[BonEnhance].value]
        #applybonus[BonAlch,container.parent,focus.field[BonAlch].value]
        #applybonus[BonComp,container.parent,focus.field[BonComp].value]
        #applybonus[BonInsight,container.parent,focus.field[BonInsight].value]
        #applybonus[BonLuck,container.parent,focus.field[BonLuck].value]
        #applybonus[BonMorale,container.parent,focus.field[BonMorale].value]
        #applybonus[BonProfane,container.parent,focus.field[BonProfane].value]
        #applybonus[BonSacred,container.parent,focus.field[BonSacred].value]
        #applybonus[BonTrait,container.parent,focus.field[BonTrait].value]
        #applybonus[Bonus,container.parent,focus.field[Bonus].value]
        ~ Specific melee bonuses
        #applybonus[atmBonAlch,container.parent,focus.field[atmBonAlch].value]
        #applybonus[atmBonComp,container.parent,focus.field[atmBonComp].value]
        #applybonus[atmBonEnh,container.parent,focus.field[atmBonEnh].value]
        #applybonus[atmBonIns,container.parent,focus.field[atmBonIns].value]
        #applybonus[atmBonLuck,container.parent,focus.field[atmBonLuck].value]
        #applybonus[atmBonMora,container.parent,focus.field[atmBonMora].value]
        #applybonus[atmBonProf,container.parent,focus.field[atmBonProf].value]
        #applybonus[atmBonSacr,container.parent,focus.field[atmBonSacr].value]
        #applybonus[atmBonTrt,container.parent,focus.field[atmBonTrt].value]
        #applybonus[atmBonus,container.parent,focus.field[atmBonus].value]
        #applybonus[atmModCirc,container.parent,focus.field[atmModCirc].value]
        #applybonus[atmRacial,container.parent,focus.field[atmRacial].value]
        ~ Specific melee penalties
        #applypenalty[atmPenComp,container.parent,focus.field[atmPenComp].value]
        #applypenalty[atmPenLuck,container.parent,focus.field[atmPenLuck].value]
        #applypenalty[atmPenMora,container.parent,focus.field[atmPenMora].value]
        #applypenalty[atmPenProf,container.parent,focus.field[atmPenProf].value]
        #applypenalty[atmPenSacr,container.parent,focus.field[atmPenSacr].value]
        #applypenalty[atmPenTrt,container.parent,focus.field[atmPenTrt].value]
        #applypenalty[atmPenalty,container.parent,focus.field[atmPenalty].value]
      endif]]></eval>
    <eval phase="Final" priority="1000"><![CDATA[
      perform hero.child[wUnarmed].setfocus
      doneif (state.isfocus = 0)
    
      ~ Set main damage
      perform delete[wMain.?]
      perform focus.pulltags[wMain.?]
      field[wDamage].value = focus.field[wDamage].value
      field[wDamNoAttr].value = focus.field[wDamNoAttr].value
      
      ~ If we're a custom/magic weapon, we need to alter our parent too
      if (container.ishero = 0) then
         container.parent.field[wDamage].value = hero.child[wUnarmed].field[wDamage].value
         container.parent.field[wDamNoAttr].value = hero.child[wUnarmed].field[wDamNoAttr].value
         perform container.parent.delete[wMain.?]
         perform container.parent.pushtags[wMain.?]
      endif]]></eval>
    </thing>
  <thing id="iS2Bane" name="Bane" description="A bane weapon excels at attacking one type or subtype of creature. Against its designated foe, its effective enhancement bonus is +2 better than its normal enhancement bonus. It deals an extra 2d6 points of damage against the foe. Bows, crossbows, and slings so crafted bestow the bane quality upon their ammunition.\n\n{b}Construction{/b}\n{b}Requirements{/b}: Craft Magic Arms and Armor, {i}summon monster I{/i}; {b}Cost{/b} +1 Bonus" compset="ItemPower" summary="+2 &amp; +2d6 damage vs chosen type" replaces="iBane" uniqueness="useronce">
    <comment>Modified to be able to turn on/off the extra damage. This is needed for the iPad app.</comment>
    <fieldval field="iPrice" value="1"/>
    <fieldval field="iCL" value="8"/>
    <fieldval field="actName" value="Attacking "/>
    <tag group="Helper" tag="Shadow" name="Shadow" abbrev="Shadow"/>
    <tag group="PowerType" tag="Weapon" name="Weapon" abbrev="Weapon"/>
    <tag group="User" tag="Activation"/>
    <tag group="iSchool" tag="Conjur" name="Conjuration" abbrev="Conjur"/>
    <tag group="Helper" tag="CustomSpec" name="CustomSpec" abbrev="CustomSpec"/>
    <eval phase="Final" priority="10000"><![CDATA[
~ If we are not active get out now!
doneif (field[abilActive].value <> 1)

~ If we are on a component
if (parent.tagis[component.BaseWep] <> 0) then
   #extradamage[parent,"+2d6 vs. " & field[iCustSpec].text,field[name].text]
else
  foreach pick in hero from BaseWep where container.child[gCustMagic].field[gWeapExpr].text
    #extradamage[eachpick,"+2d6 vs. " & field[iCustSpec].text,field[name].text]
  nexteach
endif]]></eval>
    <eval phase="PostLevel" priority="10000" index="2"><![CDATA[
~ Set activation name to be "vs Bane (NAME)"
field[actName].text &= field[iCustSpec].text

~ If we are not active get out now!
doneif (field[abilActive].value <> 1)

~ Give +2 Hit/Dmg
parent.field[Bonus].value += 2]]></eval>
    </thing>
  <thing id="abS2EldBla" name="Eldritch Blast Helper" description="Helper object to control the dice and die size of the Eldritch Blast weapon." compset="Ability">
    <usesource source="s20PFCamp"/>
    <eval phase="Final" priority="9000">~ We need to be allowed to adjust the value of the Die size from
~ outside so set the abValue1 as dice, and abValue2 as die size.
field[abValue].value += hero.child[cWitS2ElBl].field[abValue].value
field[abValue2].value += hero.child[cWitS2ElBl].field[abValue2].value</eval>
    </thing>
  <thing id="wS2GunBP" name="Firearm Bullet &amp; Powder, Crafted" description="The ammunition of most one-handed and two-handed firearms, firearm bullets typically take the form of small balls of lead or some other metal. \n\nBlack powder is the key explosive component within a firearm that enables it to function, but in larger amounts this alchemical material can be quite destructive on its own as well. A single dose of black powder is enough to power a single shot from most one-handed and two-handed firearms, while 10 doses are required to fire a cannon. Black powder is often stored and transported in kegs (which hold 100 doses), but in this quantity the powder itself becomes dangerous. Exposure to fire, electricity, or a misfire explosion causes black powder to explodeâ€”a single keg that explodes in this manner deals 5d6 points of fire damage to anyone within a 20-foot burst (DC 15 Reflex half ). Storing black powder in a powder horn protects the powder from explosion.\n\n{B}Note:{/b} This item combines the Black Powder &amp; Firearm Bullet into a single piece of ammo that was crafted using the {I}Gunsmithing{/i} feat." compset="Weapon" stacking="merge">
    <fieldval field="gWeight" value=".0167"/>
    <fieldval field="gSizeCost" value="1.10"/>
    <usesource source="UltCombat" parent="UCHeader" name="Ultimate Combat"/>
    <usesource source="CSWorldGui" parent="CSHeader" name="Inner Sea World Guide"/>
    <usesource source="UltEquip"/>
