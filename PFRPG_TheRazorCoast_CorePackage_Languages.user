  <thing id="lFGKarikan" name="Karikanti" description="Perhaps the root language for Tulita, or an ancient offshoot, the gatormen of Blacksink Marsh speak this corrupt tongue of an extinguished Tulita tribe." compset="Language" uniqueness="useronce">
    <usesource source="CampRazorC"/>

    <tag group="ProductId" tag="FrogGod"/>
    </thing>
