    <fieldval field="rSpace" value="10"/>
    <fieldval field="rReach" value="10"/>
    <fieldval field="rEnviron" value="underground"/>
    <fieldval field="rOrgan" value="standard"/>
    <fieldval field="rDescript" value="Screaming and wriggling, this foul being appears to be a pile of entwined moldering corpses."/>
    <fieldval field="rDEX" value="8"/>
    <usesource source="srcTomeHor" parent="pHLComm" name="Tome of Horrors"/>
