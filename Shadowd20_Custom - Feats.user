  <thing id="abS2RagaOb" name="+4 sacred bonus on saving throws against spells and effects cast by evil creatures" description="You gain a +4 sacred bonus on saving throws against spells and effects cast by evil creatures." compset="Ability" uniqueness="unique">
    <fieldval field="actName" value="Saves vs spells cast by evil"/>
    <usesource source="s20PFCamp"/>
    <tag group="SpecType" tag="Defense" name="Defense" abbrev="Defense"/>
    <tag group="AbilType" tag="Super" name="Supernatural Ability" abbrev=" (Su)"/>
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
~if we're not shown, just get out
doneif (tagis[Helper.ShowSpec] = 0)
~ If we're disabled, do nothing
doneif (tagis[Helper.SpcDisable] <> 0)

~ If active then set the bonus
If (field[abilActive].value <> 0) Then
   field[abValue].value += 4
   #applybonus[BonSacred,hero.child[svFort],field[abValue].value]
   #applybonus[BonSacred,hero.child[svRef],field[abValue].value]
   #applybonus[BonSacred,hero.child[svWill],field[abValue].value]
~.. If not active then set the situational text
Else
   #situational[hero.child[svAll],"+4 sacred bonus on saving throws against spells and effects cast by evil creatures.","Ragathiel Boon"]
Endif]]></eval>
    </thing>
  <thing id="rcS2VirCo2" name="Virtuous Combat: Litany of Righteousness 2/day" description="2/day use {i}litany of righteousness{/i} as a spell like ability." compset="RaceCustom" uniqueness="useronce">
