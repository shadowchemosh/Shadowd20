    <exprreq message="Shifter race required." iserror="no"><![CDATA[#hasrace[rShifters] <> 0]]></exprreq>
    </thing>
  <thing id="fShftMulti" name="Shifter Multiattack" description="You are adept at using your natural attack in conjunction with another weapon.\n\n{b}Prerequisite:{/b} Shifter with the longtooth or razorclaw trait, base attack bonus +6.\n\n{b}Benefit:{/b} Your secondary attack with a natural weapon takes only a -2 penalty.\n\n{b}Normal:{/b} Without this feat, your secondary attack with a natural weapon takes a -5 penalty." compset="Feat" summary="Your secondary attack with a natural weapon takes only a -2 penalty." uniqueness="useronce">
    <usesource source="srcShifter"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fCategory" tag="Shifter"/>
    <tag group="HasFeat" tag="fMultiAtt"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[~ If we're disabled, do nothing
doneif (tagis[Helper.FtDisable] <> 0)
~ Set the Shifter Feat Tag on the hero
perform hero.assign[Shifter.Feat]

~ Set the mulitattack tag on hero
perform hero.assign[Hero.MultiAtt]]]></eval>
    <exprreq message="Shifter with longtooth or razorclaw trait required." iserror="no"><![CDATA[hero.tagis[Shifter.Razorclaw] + hero.tagis[Shifter.Longtooth] <> 0]]></exprreq>
    <exprreq message="Must have a Base Attack Bonus of +6 or greater." iserror="no"><![CDATA[child[Attack].field[tAtkBase].value >= 6]]></exprreq>
    </thing>
  <thing id="fs20EntChr" name="Entangling Charge" description="{i}Your charge impales opponents on your deadly horns.{/i}\n\n{b}Prerequisite:{/b} Gore Attack, Powerful Charge(Racial Ability), Ulfen Minotaur\n\n{b}Benefit:{/b} When you hit an opponent with a gore attack as part of a charge, you may also start a grapple as a swift action. You do not take the -4 penalty on the combat maneuver roll if your hands are full and you do not provoke an attack of opportunity." compset="Feat" summary="Your charge entangles opponents on your deadly horns." uniqueness="useronce">
    <comment>This is a new racial feat that works with the new Ulfen Minotaur race.</comment>
    <usesource source="srcShaMino"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fCategory" tag="Racial"/>
    <exprreq message="Must have Powerful Charge Racial Ability." iserror="no"><![CDATA[#hasability[raPowCharg]<> 0]]></exprreq>
    </thing>
  <thing id="fS20ImpHor" name="Impaling Horns" description="{i}You can use your horns to impale an opponent causing them to bleed to death.{/i}\n\n{b}Prerequisite:{/b} Gore Attack, Powerful Charge(Racial Ability), Ulfen Minotaur\n\n{b}Benefit:{/b} When you hit an opponent with a gore (ie horns) charge attack, you may attempt to impale them with your horns using a Combat Maneuver vs their CMD as a swift action. This attack if successful causes the target to take 1 point of bleed damage each round plus 1 point for each 5 points in which you exceeded the targets CMD score." compset="Feat" summary="Your charge impales opponents on your horns causing bleed damge." uniqueness="useronce">
    <comment>This is a new racial feat that works with the new Ulfen Minotaur race.</comment>
    <usesource source="srcShaMino"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fCategory" tag="Racial"/>
    <exprreq message="Ulfen Minotaur race required." iserror="no"><![CDATA[hero.tagis[Race.Rd20Mino] <> 0]]></exprreq>
    <exprreq message="Must have Powerful Charge Racial Ability." iserror="no"><![CDATA[#hasability[raPowCharg]<> 0]]></exprreq>
    </thing>
  <thing id="fS2DraTail" name="Dragon Tail" description="{i}Your draconic ancestry manifests as a muscular tail you can use in combat.{/i}\n\n{b}Prerequisites:{/b} Dragonblood subtype, 1st level only.\n\n{b}Benefit:{/b} You have a tail that you can use to make a secondary natural attack in combat. This attack deals 1d6 bludgeoning damage.\n\n{b}Special:{/b} Unlike most feats, this feat must be taken at 1st level, during character creation." compset="Feat" uniqueness="useronce">
    <usesource source="srcDragonb"/>
    <tag group="fCategory" tag="Racial"/>
    <bootstrap thing="wTailSlap">
      <autotag group="wMain" tag="1d6_5"/>
      <autotag group="Value" tag="1"/>
      </bootstrap>
    <exprreq message="Dragonblood subtype required."><![CDATA[#hassubtype[stDragonBl] <> 0]]></exprreq>
    </thing>
  <thing id="fS2ImpEnRe" name="Improved Energy Resistance" description="{i}Your resistance to energy damage is greater than other members of your race.{/i}\n\n{b}Prerequisite:{/b} Dragonborn or Planetouched Race. \n\n{b}Benefit:{/b} If you are a dragonborn you may add 5 to your your racial energy resistances. If you are planetouched with energy resistance you add an amount based on your level. If between one and nine add 2, if between eleven and nineteen add 4, and at level twenty add 8 to your energy resistance." compset="Feat" summary="If you are a dragonborn you may add 5 to your your racial energy resistances. If you are planetouched with energy resistance you add an amount based on your level. If between one and nine add 2, if between eleven and nineteen add 4, and at level twenty add 8 to your energy resistance." uniqueness="useronce">
    <usesource source="srcDragonb"/>
    <usesource source="srcPlanTou"/>
    <tag group="fCategory" tag="Racial"/>
    <bootstrap thing="xDamRsCold"></bootstrap>
    <bootstrap thing="xDamRsAcid"></bootstrap>
    <bootstrap thing="xDamRsFire"></bootstrap>
    <bootstrap thing="xDamRsElec"></bootstrap>
    <eval phase="PostLevel" priority="10400"><![CDATA[
~ If we're disabled, do nothing
doneif (tagis[Helper.FtDisable] <> 0)

~Dragonborn
if (hero.childlives[raDBresist] <> 0) then
   #value[raDBresist] += 5
endif

~Planetouched
if (hero.childlives[rcPtEngRes] <> 0) then
  field[abValue].value += 2
  if (#totallevelcount[] >= 11) then
    field[abValue].value += 2
  elseif (#totallevelcount[] >= 20) then
    field[abValue].value += 6
  endif
  #value[rcPtEngRes] += field[abValue].value
endif]]></eval>
    <exprreq message="Dragonborn or Planetouched race required." iserror="no"><![CDATA[#hasrace[rDragBorn] + #hasrace[rS2Planeto] <> 0]]></exprreq>
    </thing>
  <thing id="fS2BlackCa" name="Black Cat" description="{I}Bad luck befalls those who dare to cross you.{/i}\n\n{B}Prerequisite:{/b} Catfolk.\n\n{B}Benefit:{/b} Once per day as an immediate action, when you are hit by a melee attack, you can force the opponent who made the attack to reroll it with a –4 penalty. The opponent must take the result of the second attack roll. This is a supernatural ability.\n\n{b}Special:{/b} If you take this feat and don’t already have all black fur, your fur turns completely black when you take this feat." compset="Feat" summary=" Once per day as an immediate action, when you are hit by a melee attack, you can force the opponent who made the attack to reroll it with a –4 penalty." uniqueness="useronce">
    <fieldval field="trkMax" value="1"/>
    <usesource source="srcCatFolk"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="Day"/>
    <tag group="fCategory" tag="Racial"/>
    <exprreq message="Mwangi Catfolk race required."><![CDATA[#hasrace[rS2CatFolk] <> 0]]></exprreq>
    </thing>
  <thing id="fS20MinoCh" name="Minotaur&apos;s Charge" description="{i}You can use your horns to impale an opponent causing them to bleed to death.{/i}\n\n{b}Prerequisite:{/b} Gore Attack, Powerful Charge(Racial Ability), Ulfen Minotaur\n\n{b}Benefit:{/b} When you hit an opponent with a gore (ie horns) charge attack, you may initiate a bull rush as a swift action. This does not require you to move with him for any additional movement, and does not provoke an attack of opportunity. If you successfully push your opponent back 10 feat or more, he is also knocked prone." compset="Feat" summary="When you hit an opponent with a gore (ie horns) charge attack, you may initiate a bull rush as a swift action. This does not require you to move with him for any additional movement, and does not provoke an attack of opportunity. If you successfully push your opponent back 10 feat or more, he is also knocked prone." uniqueness="useronce">
    <comment>This is a new racial feat that works with the new Ulfen Minotaur race.</comment>
    <usesource source="srcShaMino"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fCategory" tag="Racial"/>
    <exprreq message="Ulfen Minotaur race required."><![CDATA[hero.tagis[Race.Rd20Mino] <> 0]]></exprreq>
    <exprreq message="Must have Powerful Charge Racial Ability." iserror="no"><![CDATA[#hasability[raPowCharg]<> 0]]></exprreq>
    </thing>
  <thing id="fS2ZenComb" name="Zen Combat" description="Your intuition guides your hand when you use weapons.\n\n{b}Benefit{/b}: You can use your Wisdom modifier instead of your Strength modifier when using monk weapons. This bonus applies to both attack and damage.\n\n{b}Special{/b}: This feat counts as &quot;Weapon Finesse&quot; for the purposes of prerequisites." compset="Feat" summary="Use Wis instead of Str for all purposes when using Monk weapons." uniqueness="useronce">
    <comment>Maybe could help monks out I am thinking.</comment>
    <usesource source="s20PFCamp"/>
    <tag group="fCategory" tag="Combat" name="Combat" abbrev="Combat"/>
    <tag group="HasFeat" tag="fWepFin"/>
    <tag group="Helper" tag="AnimCompOK"/>
    <tag group="BonusFor" tag="Monk1"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
~ If we're disabled, do nothing
doneif (tagis[Helper.FtDisable] <> 0)

~ All Monk Weapons use Wis
foreach pick in hero from BaseWep where "wFtrGroup.Monk"
   perform eachpick.assign[DamageOpt.aWIS]
   perform eachpick.assign[MelAttOpt.aWIS]
nexteach]]></eval>
    </thing>
  <thing id="fS2EmpObed" name="Empyreal Obedience" description="{b}Prerequisites{/b}: 3 ranks in Knowledge (planes), must worship a empyreal lord.\n\n{b}Benefit{/b}: Each empyreal lord requires a different obedience, but all obediences take only an hour to perform. Once the obedience has been performed, you gain the benefit of a resistance to some element or attack associated with your empyreal lord, as indicated in the &quot;Obedience&quot; entry for the empyreal lord.\n\nIf you have at least 12 Hit Dice, you also gain the first boon granted by your empyreal lord upon undertaking your obedience. If you have at least 16 Hit Dice, you also gain the empyreal lord&apos;s second boon. If you have 20 Hit Dice or more, you also gain the empyreal lord&apos;s third and final boon. Unless a specific duration or number of uses per day is listed, a boon&apos;s effects are constant.\n\nIf you ever fail to perform a daily obedience, you lose all access to resistances and boons granted by this feat until you next perform the obedience." compset="Feat" summary="Your holy deeds prove devotion to a empyreal lord and gain benefits." uniqueness="useronce">
    <fieldval field="actName" value="Obedience Performed"/>
    <usesource source="s20PFCamp"/>
    <tag group="User" tag="Activation" name="Activation" abbrev="Activation"/>
    <tag group="Custom" tag="Obedience" name="Obedience" abbrev="Obedience"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <bootstrap thing="cfgS2EmBoo"></bootstrap>
    <eval phase="First"><![CDATA[~ If we're disabled, do nothing
doneif (tagis[Helper.FtDisable] <> 0)

~ Has the daily Obedience been performed? If so forward the tag to the 
~ hero which allows the bootstraps on each of our Configurable's specials
doneif (field[abilActive].value = 0)
perform hero.assign[Custom.Obedience]]]></eval>
    <exprreq message="Must worship a Empyreal Lord."><![CDATA[tagis[DeityCat.Empyreal] <> 0]]></exprreq>
    <exprreq message="Knowledge (Planes) 3 ranks required."><![CDATA[#skillranks[skKnowPlan] >= 3]]></exprreq>
    </thing>
  <thing id="cfgS2EmBoo" name="Empyreal Boon" description="Configurable which allows selection of least abilities." compset="Configure" uniqueness="unique">
    <fieldval field="cfgObject1" value="Lesser Boons"/>
    <fieldval field="cfgObject2" value="Greater Boons"/>
    <fieldval field="cfgObject3" value="Superior Boons"/>
    <eval phase="Final" priority="10000"><![CDATA[
      if (herofield[tHitDice].value >= 20) then
        field[cfgMax1].value += 1
        field[cfgMax2].value += 1
        field[cfgMax3].value += 1
      elseif (herofield[tHitDice].value >= 16) then
        field[cfgMax1].value += 1
        field[cfgMax2].value += 1
      elseif (herofield[tHitDice].value >= 12) then
        field[cfgMax1].value += 1
        endif]]></eval>
    </thing>
  <thing id="rcS2VirCo1" name="Virtuous Combat: Bless Weapon 3/day" description="3/day use {i}bless weapon{/i} as a spell like ability." compset="RaceCustom" uniqueness="useronce">
    <usesource source="s20PFCamp"/>
    <tag group="AllowRCust" tag="cfgS2EmBoo"/>
