    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    </thing>
  <thing id="raCTGSunWe" name="Sunlight Weakness" description="Gidims&apos; powers are weakened in natural sunlight (not merely a daylight spell), reducing the DCs of their special abilities by –4. In addition, gidims attempting to enter the Material Plane into an area of sunlight take a –4 penalty on their Will save." compset="RaceSpec" uniqueness="useronce">
