      <autotag group="Value" tag="100"/>
      </bootstrap>
    <bootstrap thing="lCommon"></bootstrap>
    <bootstrap thing="lCommonND"></bootstrap>
    <bootstrap thing="lInfernal"></bootstrap>
    <bootstrap thing="xImmCurse"></bootstrap>
