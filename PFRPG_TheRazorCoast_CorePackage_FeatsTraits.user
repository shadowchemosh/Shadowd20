    <exprreq message="Strength 15+ required."><![CDATA[#attrvalue[aSTR] >= 15]]></exprreq>
    </thing>
  <thing id="fFGEWnet" name="Exotic Weapon Proficiency (Net)" description="You understand how to use the exotic weapon net in combat, and can utilize any special tricks or qualities that exotic weapon might allow.\n\n{b}Prerequisite{/b}: Base attack bonus +1.\n\n{b}Benefit{/b}: You make attack rolls with the weapon normally.\n\n{b}Normal{/b}: A character who uses a weapon with which he is not proficient takes a &#150;4 penalty on attack rolls." compset="Feat" summary="You make attack rolls with the weapon normally." uniqueness="useronce">
    <comment><![CDATA[This is a special version of the Exotic Weapon feat that is setup to go against a specific weapon. This is currently used for the "Deep Sea Tracker" archetype.]]></comment>
    <usesource source="CampRazorC"/>
    <tag group="Target" tag="wNet"/>
    <tag group="HasFeat" tag="fExoticWep"/>
    <tag group="ChooseSrc1" tag="Thing" name="All Things" abbrev="All Things"/>
    <tag group="fCategory" tag="Combat" name="Combat" abbrev="Combat"/>
    <tag group="fShowWhat" tag="WeaponsExo" name="Exotic Weapons" abbrev="Exotic Weapons"/>
    <tag group="ProductId" tag="FrogGod"/>
    <tag group="Helper" tag="Helper"/>
    <eval phase="First" priority="1000"><![CDATA[
~ If we're disabled, do nothing
doneif (tagis[Helper.FtDisable] <> 0)

var v_tag as string
~ Set parm with the exotic prof tag and set for the ONLY a net weapon
v_tag = "Helper.ExoticProf|thingid.wNet"
~ Reset live name so we don't get (Net)(Net) appeneded to feat name
field[livename].text = "Exotic Weapon Proficiency"
call ChosenProf]]></eval>
    <evalrule phase="PostAttr" priority="10000" message="Ability requirement not met" summary="Ability requirement not met"><![CDATA[
      ~ If we're disabled, do nothing, we're valid
      validif (tagis[Helper.FtDisable] <> 0)

      ~ If we haven't chosen anything, get out
      @valid = 1
      doneif (field[usrChosen1].ischosen = 0)

      ~ Check the attribute requirements for taking an exotic weapon
      ~ proficiency
      var v_req as number
      var v_current as number
      var v_attr as string
      v_req = field[usrChosen1].chosen.tagvalue[StrReq.?]
      v_current = #attrvalue[aSTR]
      v_attr = "Strength"
      call fExoticAtt
      v_req = field[usrChosen1].chosen.tagvalue[DexReq.?]
      v_current = #attrvalue[aDEX]
      v_attr = "Dexterity"
      call fExoticAtt
      v_req = field[usrChosen1].chosen.tagvalue[ConReq.?]
      v_current = #attrvalue[aCON]
      v_attr = "Constitution"
      call fExoticAtt
      v_req = field[usrChosen1].chosen.tagvalue[IntReq.?]
      v_current = #attrvalue[aINT]
      v_attr = "Intelligence"
      call fExoticAtt
      v_req = field[usrChosen1].chosen.tagvalue[WisReq.?]
      v_current = #attrvalue[aWIS]
      v_attr = "Wisdom"
      call fExoticAtt
      v_req = field[usrChosen1].chosen.tagvalue[ChaReq.?]
      v_current = #attrvalue[aCHA]
      v_attr = "Charisma"
      call fExoticAtt]]>
      <after name="Attribute Final"/>
      </evalrule>
    <exprreq message="Base attack bonus +1 required." iserror="no"><![CDATA[child[Attack].field[tAtkBase].value >= 1]]></exprreq>
    </thing>
  <thing id="fFGQuicCha" name="Quick Change" description="You may change forms in the blink of an eye, flesh flowing like quicksilver.\n\n{b}Prerequisites{/b}: Must possess the alternate form ability, or the ability to change shape (such as the doppelgangers shapechange ability, a polymorph spell, or a druid&#146;s wild shape class feature).\n\n{b}Benefit{/b}: You may change shape or form as a swift action as opposed to a standard action." compset="Feat" summary="Change shape or form as a swift action." uniqueness="useronce">
