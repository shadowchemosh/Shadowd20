  <thing id="fKQFavTerr" name="Favored Terrain Expert" description="You are especially at home in a specific favored terrain.\n\n{b}Prerequisite:{/b} Favored terrain class ability\n\n{b}Benefit:{/b} You may specify one of your favored terrains. While in that terrain, you gain an additional +2 bonus to initiative and to Knowledge (geography), Perception, Stealth, and Survival skill checks.\n\n{b}Note:{/b} The scripting for this only gives you an upgrade to apply to your Favored Terrain.  You must manually choose the Favored Terrain to upgrade." compset="Feat" summary="Gain an additional +2 bonus to a favored terrain." uniqueness="useronce">
    <comment>KQ#11</comment>
    <usesource source="s20PFCamp"/>
    <tag group="Helper" tag="ShowSpec" name="ShowSpec" abbrev="ShowSpec"/>
    <tag group="ProductId" tag="KoboldPres"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <eval phase="PostAttr" priority="10000"><![CDATA[
~ If we're disabled, do nothing
doneif (tagis[Helper.FtDisable] <> 0)

~ Give a terrain upgrade
hero.child[resTerrUpg].field[resMax].value += 1]]></eval>
    <exprreq message="Favored terrain class ability required." iserror="no"><![CDATA[#resmax[resTerrSel] <> 0]]></exprreq>
    </thing>
  <thing id="cRslTrack" name="Track" description="A ranger adds half his level (minimum 1) to Survival skill checks made to follow or identify tracks." compset="ClSpecial" summary="Bonus to all survival checks made to follow or identify tracks.">
    <comment><![CDATA[This is abilit has been "Hidden" as we are now using the "CORE" track ability.]]></comment>
    <eval phase="Render" priority="1000"><![CDATA[
      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)
      ~ Only do it for the first copy.
      doneif (tagis[Helper.FirstCopy] = 0)

      ~ If we have the Natural Tracker feat we are full bonus
      ~ not half.
      If (#hasfeat[fKQNatTrck] <> 0) Then
        ~ our bonus is our level
        field[abValue].value += field[xAllLev].value
      Else      
        ~ our bonus is level /2, min 1
        field[abValue].value += maximum(round(field[xAllLev].value / 2,0,-1),1)
      Endif  
      
      ~ Set the Live Name
      field[livename].text = field[thingname].text & " " & signed(field[abValue].value)
      
      ~ Give situational text to Survival
      #situational[hero.child[skSurvival],signed(field[abValue].value) & " to follow or identify tracks.",field[thingname].text]
      
      ~ Set tag on hero so we are the same as the CORE ranger track
      perform hero.assign[HasAbility.cRgrTrack]
      perform hero.assign[Ability.cRgrTrack]]]></eval>
    </thing>
  <thing id="fKQAddFavE" name="Additional Favored Enemy" description="You have additional favored enemies.\n\n{b}Prerequisite:{/b} Favored enemy class ability\n\n{b}Benefit:{/b} You may choose one additional favored enemy. All bonuses are at base amounts but may be increased following the favored enemy ability rules." compset="Feat" summary="You may choose one additional favored enemy. All bonuses are at base amounts but may be increased following the favored enemy ability rules." uniqueness="useronce">
