  <thing id="cRslStlSnp" name="Stealthy Sniper" description="With this talent, when using the Stealth skill to snipe, you suffer only a -15 penalty (or -10 when in a favored terrain) on your second Stealth check instead of the normal -20." compset="CustomSpec" uniqueness="useronce">
    <usesource source="s20PFCamp"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev="(Ex)"/>
