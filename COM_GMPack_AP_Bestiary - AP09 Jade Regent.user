    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="Final">field[trkMax].value = herofield[tHitDice].value</eval>
    <eval phase="PostLevel" priority="10000" index="2">if (field[abilActive].value = 0) then
  ~ disable incorporeal
    hero.childfound[stIncorpor].activated = 0
    perform hero.childfound[raIncorpor].assign[Helper.SpcDisable]
    perform hero.childfound[raIncorpor].assign[Hide.Special]
    perform hero.childfound[xImmCrit].assign[Helper.SpcDisable]
    perform hero.childfound[xImmCrit].assign[Hide.Special]
    perform hero.childfound[xImmPrec].assign[Helper.SpcDisable]
    perform hero.childfound[xImmPrec].assign[Hide.Special]
  ~ add STR
    perform hero.child[aSTR].delete[Helper.NoScore]
    perform hero.child[aSTR].delete[AttrSub.aDEX]
 
else
  ~ enable incorporeal
    hero.childfound[stIncorpor].activated = 1 
    perform hero.childfound[raIncorpor].delete[Helper.SpcDisable]
    perform hero.childfound[raIncorpor].delete[Hide.Special]
    perform hero.childfound[xImmCrit].delete[Helper.SpcDisable]
    perform hero.childfound[xImmCrit].delete[Hide.Special]
    perform hero.childfound[xImmPrec].delete[Helper.SpcDisable]
    perform hero.childfound[xImmPrec].delete[Hide.Special]
  ~ remove STR
    perform hero.child[aSTR].assign[Helper.NoScore]
    perform hero.child[aSTR].assign[AttrSub.aDEX]
  
endif</eval>
    <eval phase="PostAttr" priority="2000" index="3">if (field[abilActive].value = 0) then
  perform hero.child[mNatural].delete[Helper.NoArmor]
  ~ remove deflection bonus
      var bonus as number
      bonus = hero.child[aCHA].field[aModBonus].value
      bonus = maximum(bonus, 1)
      hero.child[ArmorClass].field[tACDeflect].value -= maximum(hero.child[ArmorClass].field[tACDeflect].value, bonus)
else
~ Set our natural armor bonus to +0 - must be added before PostAttr 5000
      perform hero.child[mNatural].assign[Helper.NoArmor]

      ~ Add our charisma bonus as a deflection bonus
      var bonus as number
      bonus = hero.child[aCHA].field[aModBonus].value
      bonus = maximum(bonus, 1)
      hero.child[ArmorClass].field[tACDeflect].value = maximum(hero.child[ArmorClass].field[tACDeflect].value, bonus)

endif</eval>
    </thing>
