    <eval phase="First"><![CDATA[      ~ Disable all special abilities apart from the ones that we bootstrap and extraordinary ones that are attacks
      var searchexpr as string
      searchexpr = "!component.BaseFeat & !SpecType.Movement & !thingid.typUndead & !(AbilType.Extra & SpecType.Attack) & !SpecSource.tmuFrostfa & !thingid.xVulFire & !thingid.xImmCold"
        foreach pick in hero from Ability where searchexpr
          perform eachpick.assign[Helper.SpcDisable]
          perform eachpick.assign[Hide.Special]
          nexteach

      ~ Go over all weapons, deleting stuff like weapon focus and
      ~ improved natural attack from them - these tags are often just
      ~ bootstrapped directly onto the weapon
      foreach pick in hero from BaseWep
        perform eachpick.delete[Broadcast.?]
        nexteach

foreach pick in hero from BaseWep where "thingid.wSlam & !SpecSource.tmuFrostfa"
   perform eachpick.assign[Hide.Weapon]
nexteach]]></eval>
    <eval phase="PreLevel" priority="10000" index="5"><![CDATA[ ~ We don't have an INT score
      perform hero.child[aINT].assign[Helper.NoScore]
~ Force our Wisdom to 10 a
      hero.child[aWIS].field[aNormForce].value = 10

var hd as number
   hd = hero.tagcount[Hero.HitDice] - hero.tagcount[Classes.?]


if (hd >= 11) then
  perform hero.child[xDamRdB].delete[Value.?]
  perform hero.child[xDamRdB].assign[Value.10]
else
  perform hero.child[xDamRdB].delete[Value.?]
  perform hero.child[xDamRdB].assign[Value.5]
endif

if (hd >= 16) then
  hero.child[raColdBody].field[livename].text = "Cold (4d6)"
  done
elseif (hd >= 11) then
  hero.child[raColdBody].field[livename].text = "Cold (3d6)"
  done
elseif (hd >=6) then
  hero.child[raColdBody].field[livename].text = "Cold (2d6)"
  done
else
  hero.child[raColdBody].field[livename].text = "Cold (1d6)"
endif]]></eval>
    <eval phase="Final" priority="9000" index="4"><![CDATA[if (hero.childfound[xFly].tagis[Helper.NotNatural] = 0) then

      ~ If flight is natural delete all maneuverability tags from flight and make clumsy
      perform hero.childfound[xFly].delete[Maneuver.?]
      perform hero.childfound[xFly].assign[Maneuver.Clumsy]

endif

~ remove spells

perform hero.delete[Hero.Caster]

foreach pick in hero where "component.BaseSpell"
  ~perform eachpick.assign[Helper.SpOverLev]
  perform eachpick.assign[Hide.Spell]
  ~perform eachpick.delete[KnowSpell.?]
nexteach

~ remove spell-like ability spells

foreach pick in hero where "Helper.SpellLike"
 perform eachpick.assign[Helper.SpcDisable]
 perform eachpick.assign[Hide.Special]
nexteach

~ Remove languages

foreach pick in hero where "component.BaseLang"
 perform eachpick.assign[Hide.Language]
nexteach

perform hero.delete[SpeakLang.?]
perform hero.assign[Hero.CantSpeak]

herofield[tLangsSta].value = 0
herofield[tLangsSpk].value = 0]]></eval>
    <eval phase="Final" index="6"><![CDATA[      ~ Many races and classes bootstrap feats, so go through them disabling them all - except Toughness!
     foreach pick in hero from BaseFeat where "!thingid.fToughness"
       perform eachpick.assign[Helper.FtDisable]
       perform eachpick.assign[Helper.FtHide]
       perform eachpick.assign[thing.skipprereq]
       perform eachpick.delete[User.?]
       perform eachpick.delete[Activated.?]
     nexteach]]></eval>
    <eval phase="First" priority="530" index="3">~if we have no racial HD, add one to our template
      if (hero.tagcount[Hero.HitDice] = 0) then
        field[tmHitDice].value += 1
        endif</eval>
