    <exprreq message="Medium Armor proficiency is required."><![CDATA[#hasfeat[fArmMedium] <> 0]]></exprreq>
    <exprreq message="Caster level 11 is required."><![CDATA[herofield[tMaxCaster].value >= 11]]></exprreq>
    <exprreq message="Arcane Armor Training feat is required."><![CDATA[#hasfeat[fArcArmTra] <> 0]]></exprreq>
    </thing>
  <thing id="fSGBckSwgA" name="Backswing Attack" description="When your attacks bounce off a foe’s armor, you can use the momentum to backswing against another target. \n\n{b}Prerequisites{/b}: Base attack bonus +6. \n\n{b}Benefit{/b}: Three times per day, as a free action, when you make a weapon attack, unarmed attack, or natural weapon attack, and miss your target (but successfully hit the target’s touch AC), you may immediately reroll the attack as an attack against another target. This attack counts as the same attack for purposes of abilities that only work on a single attack roll, and the new target must be one you could have attacked instead of your original target." compset="Feat" uniqueness="useronce">
    <fieldval field="trkMax" value="3"/>
    <usesource source="SGG_MOverP"/>
    <tag group="fCategory" tag="MetaAttack"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="fCategory" tag="Overpowerd"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="Day"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Base Attack Bonus +6 is required."><![CDATA[child[Attack].field[tAtkBase].value >= 6]]></exprreq>
    </thing>
  <thing id="fSGBrowCut" name="Brow Cut" description="You cut a foe’s brow causing blood from the wound to flow into its eyes. \n\n{b}Benefit{/b}: Once per round as a free action, when you hit and damage a foe with a slashing weapon, you cause the foe to be dazzled for one round, +1 round per 5 points of your base attack bonus. Any magic healing directed specifically at healing the wound bleeding  into the target’s eyes ends this effect, though it does not also restore hit points." compset="Feat" uniqueness="useronce">
    <usesource source="SGG_MOverP"/>
    <tag group="fCategory" tag="Overpowerd"/>
    <tag group="fCategory" tag="MetaAttack"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    </thing>
  <thing id="fSGEyeGoug" name="Eye Gouge" description="You cut a foe’s brow causing blood from the wound to flow into its eyes. \n\n{b}Prerequisites{/b}: Improved Unarmed Strike, Improved Grapple\n\n{b}Benefit{/b}: Once per round as a free action, when you hit and damage a foe with an unarmed attack, you cause the foe to be dazzled for one round, +1 round per 5 points of your base attack bonus. Any magic healing directed specifically at healing the wound bleeding  into the target’s eyes ends this effect, though it does not also restore hit points." compset="Feat" uniqueness="useronce">
    <usesource source="SGG_MOverP"/>
    <tag group="fCategory" tag="Overpowerd"/>
