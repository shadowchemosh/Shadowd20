    </thing>
  <thing id="fSGChnlBrt" name="Channel Brightly" description="You can channel energy of an impressive intensity.\n\n{b}Prerequisite{/b}: Ability to channel energy, Improved Channel, Selective Channeling. \n\n{b}Benefit{/b}: When you channel energy, you may simultaneously heal all creatures you can heal with the ability, and harm all creatures you could harm with the ability." compset="Feat" summary="You can channel energy of an impressive intensity." uniqueness="useronce">
    <usesource source="SGG_MOverP"/>
    <tag group="fCategory" tag="Overpowerd"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Selective Channeling feat is required."><![CDATA[#hasfeat[fSelChan] <> 0]]></exprreq>
