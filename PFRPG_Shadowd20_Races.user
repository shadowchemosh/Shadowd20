  <thing id="fcOraPlane" name="Planetouched Oracle" description="Add one spell known from the oracle spell list. This spell must be at least one level below the highest spell level the oracle can cast.\n\n{b}Note{/b}: A new copy of this ability should be added for each time you wish to select it.  That way, each copy can be set to grant a different spell level.  On the In-Play tab, select the level this copy applies to.  This will increase the count of Oracle spells known for that level, allowing you to add the spell you want." compset="FavClass" uniqueness="useronce">
    <fieldval field="fcName" value="+1 Spell Known"/>
    <fieldval field="actUserMin" value="0"/>
    <fieldval field="actUserMax" value="0"/>
    <usesource source="srcPlanTou"/>
    <tag group="FavClass" tag="cOracle"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="IsRace" tag="rS2Planeto"/>
    <eval phase="PostLevel" priority="500">~Post-levels 500
      perform assign[Helper.SpecUp]

      field[actUserMin].value += maximum(hero.childfound[cHelpOra].field[cSplMinLvl].value, 0)
      
      field[actUserMax].value += maximum(hero.childfound[cHelpOra].field[cSplMaxLvl].value - 1, 0)</eval>
    <evalrule phase="Validation" priority="5000" index="2" message="You should only select this ability with a single favored class level.  For additional levels, add more copies of this ability.  That way, each time it is selected, it can be applied to a different level." summary="You should only select this ability with a single favored class level.  For additional levels, add more copies of this ability.  That way, each time it is selected, it can be applied to a different level."><![CDATA[~Validation Phase 5,000
      validif (field[fcCount].value <= 1)]]></evalrule>
    <evalrule phase="Final" priority="10000" message="You have chosen to add a spell level that is above the maximum level of spells you can cast." summary="Selected spell level too high."><![CDATA[~Final Phase 10,000
      if (field[fcCount].value >= 1) then
        var spelllevel as number
        spelllevel = maximum(hero.childfound[cHelpOra].field[cMaxSpLev].value - 1, 0)
        
        if (spelllevel >= field[actUser].value) then
          @valid = 1
          endif
        
        spelllevel = minimum(spelllevel, field[actUser].value)
        
        field[fcName].text &= ": " & spelllevel
  
        hero.childfound[cHelpOra].field[cKnowMax].arrayvalue[spelllevel] += 1
      else
        @valid = 1
        field[fcName].text &= ": " & field[actUser].value
        endif]]></evalrule>
    </thing>
