  <thing id="raIgnNatAt" name="Tail Swipe" description="{b}Natural Attack:{/b} For 1d4 nonlethal damage and free trip attack (10ft range). You are not knocked prone even if your trip attack fails by 10 or more. A Tail Swipe is nonlethal damage and it deals no damage to any creature with an armor bonus of +1 or higher or a natural armor bonus of +3 or higher. A Tail Swipe is treated as a melee weapon with 10-foot reach, though you don&#146;t threaten the area into which you can make an attack. In addition, unlike most other weapons with reach, you can use it against foes anywhere within your reach (including adjacent foes). All other normal rules for Natural Attacks work per core rules." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Attack"/>
    </thing>
  <thing id="raAnimalin" name="Animal Instincts" description="The shifter&apos;s innate understanding of the natural world allows him to add either his Wisdom modifier or his Charisma modifier, which ever is higher, to his wild empathy checks." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fShowWhat" tag="Skills"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <eval phase="PreLevel" priority="5000"><![CDATA[~make class skills

if (field[usrChosen1].ischosen <> 0) then
   perform field[usrChosen1].chosen.assign[Helper.ClassSkill]
endif

if (field[usrChosen2].ischosen <> 0) then
   perform field[usrChosen2].chosen.assign[Helper.ClassSkill]
endif]]></eval>
    </thing>
  <thing id="raChangeMe" name="Shape Change(Medium)" description="If the form you take is that of a Medium humanoid, you gain a +2 size bonus to your Strength." compset="RaceSpec">
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="PreAttr" priority="10000">~ If we are not activated get out now
doneif (field[abilActive].value = 0)

~ Add a +2 shape change bonus to our Strength
hero.child[aSTR].field[aStartMod].value += 2</eval>
    <eval phase="PreLevel" priority="5000" index="2">~ If we are not activated get out now
doneif (field[abilActive].value = 0)

#applybonus[ModCirc,hero.child[skDisguise],10]</eval>
    </thing>
  <thing id="raChangeSm" name="Shape Change(Small)" description="If the form you take is that of a Small humanoid, you gain a +2 size bonus to your Dexterity." compset="RaceSpec">
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="PreAttr" priority="10000">~ If we are not activated get out now
      doneif (field[abilActive].value = 0)

~ Add a +2 shape change bonus
hero.child[aDEX].field[aStartMod].value += 2</eval>
    <eval phase="First" priority="20000" index="2"><![CDATA[~ If we are not activated get out now
doneif (field[abilActive].value = 0)

~ Add to size - must come after race and template size set
herofield[tSize].value -= 1
~ +10 circumstance bonus to disguise skill
#applybonus[ModCirc,hero.child[skDisguise],10]

~ Decrease the size of the unarmed strike
~foreach pick in hero from MyGear where "wCategory.Unarmed"
~    eachpick.field[gSizeMod].value -= 1
~nexteach]]></eval>
    </thing>
  <thing id="raS2PtSp" name="Spell-like Abilities" description="A planetouched can use the spells {i}Detect Evil, Detect Magic, Light, or Stabilize{/i} a number of times per day equal to 1/2 your character level (Minimum 1/day).  The Caster Level for the spells is 1/2 your character level (Minimum CL of 1)." compset="RaceSpec" summary="Detect Evil, Detect Magic, Light, and Stabilize.">
    <tag group="AbilType" tag="SpellLike"/>
    <tag group="Helper" tag="ShowSpec"/>
    <bootstrap thing="spDeteEvi1">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Usage" tag="Day"/>
      <autotag group="Shadowd20" tag="PtSp"/>
      </bootstrap>
    <bootstrap thing="spDeteMag0">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Usage" tag="Day"/>
      <autotag group="Shadowd20" tag="PtSp"/>
      </bootstrap>
    <bootstrap thing="spLight0">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Usage" tag="Day"/>
      <autotag group="Shadowd20" tag="PtSp"/>
      </bootstrap>
    <bootstrap thing="spStabili0">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Usage" tag="Day"/>
      <autotag group="Shadowd20" tag="PtSp"/>
      </bootstrap>
    <eval phase="PostLevel" priority="10500"><![CDATA[
~ Calc CL and uses per day
field[abValue].value += maximum(round(#totallevelcount[]/2,0,-1),1)
~ Set the Caster Level
field[livename].text = field[thingname].text & " CL" & field[abValue].value

~ Read through all the Planetouched Sp and set the CL and uses
foreach pick in hero from BaseSpell where "Shadowd20.PtSp"
   eachpick.field[trkMax].value += field[abValue].value
   eachpick.field[sCL].value += field[abValue].value
nexteach]]></eval>
    </thing>
  <thing id="raPtCharDe" name="Character Description" description="As displayed by the above pictures Planetouched come in a wide variety of appearances. You are allowed to describe your character pretty much as you see fit except that you can not easily pass as human. Everyone knows there is something just not right about your character." compset="RaceSpec"></thing>
  <thing id="raPtAlignm" name="Alignment" description="Evil versions of Planetouched are for DM&apos;s use only. While a planetouched does not have to be of good alignment; neutral and evil planetouched do NOT gain access to the {i}Aura of Good{/i} ability listed below." compset="RaceSpec"></thing>
  <thing id="raPtAuraGo" name="Aura of Good" description="This power is only active for characters with {i}good{/i} alignments either Lawful Good; Neutral Good; or Chaotic Good. The power of a planetouched&apos;s aura of good (see the detect good spell) is equal to her character level. Your aura of good precedes you and others are affected by it.\n-Neutral NPCs always treat you with a starting value of Indifferent.\n-Good NPCs always treat you with a starting value of Friendly.\n-Evil NPCs always treat you with a starting value of Unfriendly." compset="RaceSpec" summary="The power of a planetouched&apos;s aura of good (see the detect good spell) is equal to her character level. Your aura of good precedes you and others are affected by it.">
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="PostAttr" priority="10000"><![CDATA[~ If we are not good then we are disabled
If (hero.tagis[Alignment.Good] <> 1) Then
   perform assign[Helper.SpcDisable]
   perform delete[Helper.ShowSpec]
Endif]]></eval>
    </thing>
  <thing id="rcPtAbilit" name="Ability Adjustments" description="+2 to either Int, Wis, or Cha. This counts as race type." compset="RaceCustom" uniqueness="useronce">
