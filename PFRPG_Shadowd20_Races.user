  <thing id="rcPtEngRes" name="Energy Resistance" description="Resistance 3 against one specific elemental type chosen at level 1. Once chosen this can not be changed. This increases to Resistance 6 at level ten and Resistance 12 at level twenty." compset="RaceCustom" uniqueness="unique">
    <usesource source="s20PFCamp"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="AllowRCust" tag="rS2Planeto"/>
    <tag group="fShowWhat" tag="Energy"/>
    <bootstrap thing="xDamRsCold"></bootstrap>
    <bootstrap thing="xDamRsFire"></bootstrap>
    <bootstrap thing="xDamRsAcid"></bootstrap>
    <bootstrap thing="xDamRsElec"></bootstrap>
    <eval phase="PostLevel" priority="10500"><![CDATA[~Post-Levels 10,500
var Lvl as number
Lvl = #totallevelcount[]
field[abValue].value += 3
If (Lvl >= 10) then
   field[abValue].value += 3
endif
If (Lvl >= 20) then
   field[abValue].value += 6
endif

~Apply the correct type of resistance to the Dragonborn
if (field[usrChosen1].chosen.pulltags[thingid.selEnAcid] = 1) then
      #applyresist[xDamRsAcid,field[abValue].value]
endif
if (field[usrChosen1].chosen.pulltags[thingid.selEnElec] = 1) then
      #applyresist[xDamRsElec,field[abValue].value]
endif
if (field[usrChosen1].chosen.pulltags[thingid.selEnFire] = 1) then
      #applyresist[xDamRsFire,field[abValue].value]
endif
if (field[usrChosen1].chosen.pulltags[thingid.selEnCold] = 1) then
      #applyresist[xDamRsCold,field[abValue].value]
endif]]></eval>
    </thing>
  <thing id="rcPtSp1" name="Spell-Like ability" description="One additional spell-like ability chosen from the list below. Once chosen this can not be changed. It functions once per day plus one additional time per 5 character levels(ie twice at level 5, three times at level 10). Caster Level equals character level.\n\n{I}Aid, Cure Moderate Wounds, Daylight, Darkness, Inflict Moderate Wounds, Protection from Evil, Protection from Chaos, and Protection from Law.{/I}" compset="RaceCustom" uniqueness="useronce">
    <fieldval field="trkMax" value="1"/>
    <fieldval field="usrCandid1" value="thingid.spDayligh3|thingid.spDarknes2|thingid.spProtEvi1|thingid.spProtfro1|thingid.spProtLaw1|thingid.spAid2|thingid.spCureMod2|thingid.spInflMod2"/>
    <fieldval field="shortname" value="Sp"/>
    <tag group="AllowRCust" tag="rS2Planeto"/>
    <tag group="Usage" tag="Day"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <tag group="AbilType" tag="SpellLike"/>
    <tag group="User" tag="Tracker"/>
    <eval phase="PostLevel" priority="10500"><![CDATA[~Post-Levels 10,500
~ Add additional daily use per 5 HD
field[abValue].value = round(#totallevelcount[]/5,0,-1)
field[trkMax].value += field[abValue].value

~ Calc Caster Level
field[abValue2].value += #totallevelcount[]
field[livename].text = field[thingname].text & " CL" & field[abValue2].value]]></eval>
    </thing>
