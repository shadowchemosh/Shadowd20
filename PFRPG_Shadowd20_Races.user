  <thing id="raS2TenSwo" name="Swordtrained" description="Tengus are trained from birth in swordplay, and as a result are automatically proficient with swordlike weapons." compset="RaceSpec">
    <tag group="AbilType" tag="Extra"/>
    <eval phase="First" priority="20000"><![CDATA[
~if we've been replaced, get out now
doneif (tagis[Helper.SpcReplace] <> 0)

~ Build all tags on us
foreach thing in BaseWep where "wFtrGroup.BladeLight | wFtrGroup.BladeHeavy"
    perform eachthing.pulltags[WepProf.?]
nexteach
~ push them all to hero
perform hero.pushtags[WepProf.?]
      
~ Cycle through and apply the Helper.ExoticProf tag to all 
~ exotic weapons.
var sTags as string
~ Pull all the weapon tags 
sTags = tagids[WepProf.?,"|"]
~ Loop through all the weapon picks and set hepler tag
foreach pick in hero from BaseWep where sTags
   perform eachpick.assign[Helper.ExoticProf]
nexteach]]></eval>
