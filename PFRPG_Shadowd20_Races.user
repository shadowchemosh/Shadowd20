    <tag group="Language" tag="lDwarven"/>
    <tag group="Language" tag="lElven"/>
    <tag group="Helper" tag="OptCustom"/>
    <bootstrap thing="raDBbrthw"></bootstrap>
    <bootstrap thing="fcPalDragB"></bootstrap>
    <bootstrap thing="fcClrDragB"></bootstrap>
    <bootstrap thing="fcSorDragB"></bootstrap>
    <bootstrap thing="raDBresist"></bootstrap>
    <bootstrap thing="raDBaspBl"></bootstrap>
