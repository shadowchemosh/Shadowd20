  <thing id="fcOraS2Kit" name="Kitsune Oracle S2" description="Reduce the penalty for not being proficient for one weapon by 1. When the non-proficiency penalty for a weapon becomes 0 because of this ability, the oracle is treated as having the appropriate Martial or Exotic Weapon Proficiency feat for that weapon." compset="FavClass">
    <fieldval field="fcName" value="-1 N-Prof Penalty"/>
    <fieldval field="usrCandid1" value="component.BaseWep &amp; !Helper.NoSelect &amp; !component.Specific &amp; !wCategory.Amm? &amp; (wProfReq.Exotic | wProfReq.Martial)"/>
    <usesource source="srcDragEmp"/>
    <tag group="IsRace" tag="rKitsune" name="Kitsune" abbrev="Kitsune"/>
    <tag group="ChooseSrc1" tag="Thing" name="All Things" abbrev="All Things"/>
    <tag group="FavClass" tag="cOracle" name="Oracle" abbrev="Oracle"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <evalrule phase="PreLevel" priority="5000" message="There is no benefit to selecting this favored class option more than 4 times for each weapon." summary="There is no benefit to selecting this favored class option more than 4 times for each weapon." severity="warning"><![CDATA[ field[abValue].value += field[fcCount].value

      validif (field[abValue].value = 0)

      ~if we haven't chosen anything, there's nothing else we can do
      validif (field[usrChosen1].ischosen = 0)

      perform field[usrChosen1].chosen.pulltags[IsWeapon.?]

      if (hero.intersect[WepProf,IsWeapon] <> 0) then
        @message = "Something else has granted you proficiency with the selected weapon.  This ability has no effect."
      elseif (field[abValue].value > 4) then
        perform field[usrChosen1].chosen.pulltags[WepProf.?]
        perform forward[WepProf.?]
      elseif (field[abValue].value = 4) then
        perform field[usrChosen1].chosen.pulltags[WepProf.?]
        perform forward[WepProf.?]
        @valid = 1
      else
        @valid = 1
        ~find all the weapons that match the one we've chosen and add a bonus
        ~(to the same fields the non-proficiency penalty is being added to)
        var searchexpr as string
        searchexpr = tagids[IsWeapon.?,"|"]
        foreach pick in hero from BaseWep where searchexpr
          eachpick.field[wAttack].value += field[abValue].value
          eachpick.field[wRanAtk].value += field[abValue].value
          nexteach
        endif]]></evalrule>
    </thing>
