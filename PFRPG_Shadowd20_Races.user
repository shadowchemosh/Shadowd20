  <thing id="fcGunS2Rat" name="Ratfolk Gunslinger" description="Add a +1/2 bonus on initiative checks when the gunslinger has at least 1 grit point." compset="FavClass" uniqueness="unique">
    <fieldval field="fcName" value="+&#189; Initiative with Grit."/>
    <usesource source="s20PFCamp"/>
    <tag group="FavClass" tag="cGunsling" name="Gunslinger" abbrev="Gunslinger"/>
    <tag group="IsRace" tag="rRatfolk" name="Ratfolk" abbrev="Ratfolk"/>
    <eval phase="Final" priority="2000"><![CDATA[
      field[abValue].value += round(field[fcCount].value/2,0,-1)

      perform hero.findchild[BaseClSpec,"thingid.cGunGunIni"].setfocus

      doneif (state.isfocus = 0)

      ~ If the chosen ability is not yet active, add to initiative ourselves. Otherwise add to the value of this ability.
      if (focus.tagis[Helper.ShowSpec] = 0) then
        hero.child[Initiative].field[Bonus].value += field[abValue].value
      else
        focus.field[abValue].value += field[abValue].value
        endif]]></eval>
    </thing>
  <thing id="fcMnkS2Rat" name="Ratfolk Monk" description="Add +1 feet to the speed the monk can move while making a Stealth check without taking a penalty. This has no effect unless the monk has selected this reward five times (or another increment of five). This does not allow the monk to use Stealth while running or charging." compset="FavClass" uniqueness="unique">
    <fieldval field="fcName" value="+1 Max Stealth Speed "/>
    <usesource source="s20PFCamp"/>
    <tag group="FavClass" tag="cMonk" name="Monk" abbrev="Monk"/>
    <tag group="IsRace" tag="rRatfolk" name="Ratfolk" abbrev="Ratfolk"/>
    <eval phase="PostAttr" priority="11000"><![CDATA[
      field[abValue].value += field[fcCount].value
      
      field[abValue2].value += field[abValue].value % 5
