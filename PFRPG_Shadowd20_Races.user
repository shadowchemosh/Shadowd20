  <thing id="raS2MinCun" name="Cunning" description="Some groups of ulfen are famed for being particularly cunning, with their members rarely ever being fooled or becoming lost. Gain a +2 bonus to all Wisdom checks and Wisdom-based skill checks. This ability replaces the Stubborn racial ability." compset="AltRaceTrt" uniqueness="unique">
    <usesource source="s20PFCamp"/>
    <tag group="AllowRCust" tag="Rd20Mino"/>
    <tag group="RaReplace" tag="raS2Stubbo"/>
    <tag group="Helper" tag="SpecUp"/>
    <eval phase="PostLevel" priority="20000"><![CDATA[
~ Set to +2 bonus
field[abValue].value += 2

#racialsituational[hero.child[aWIS],signed(field[abValue].value) & " to all Wisdom checks",field[name].text]
var nValid as number

~ Give penalty to Wis based skills
foreach pick in hero from BaseSkill
  nValid = 0
  ~ If we have a skill override then process here
  if (eachpick.tagcount[SkillOver.?] <> 0) then
     ~ Check Skill Overrides
     if (eachpick.tagis[SkillOver.aWIS] <> 0) then
        nValid += 1
     endif
  ~ if we don't have a skill override then process here
  else
     ~ Check normal skill attributes
     if (eachpick.islinkage[skillattr] <> 0) then
        if (eachpick.linkage[skillattr].tagis[thingid.aWIS] <> 0) then
           nValid += 1
        endif
     endif
  endif

  ~ If nValid skill then apply the bonus
  if (nValid <> 0) then
     eachpick.field[Bonus].value += field[abValue].value
  endif
nexteach]]></eval>
    </thing>
