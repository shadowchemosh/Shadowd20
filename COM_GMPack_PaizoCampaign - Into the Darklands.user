  <thing id="lsakvroth" name="Sakvroth" description="A secret language of silent hand gestures useful at close range by creatures with prehensile digits. It is most commonly used by individuals with darkvision in order to communicate discreetly without the need for light or noise." compset="Language" uniqueness="useronce">
    <usesource source="srcIntDark"/>
    <tag group="Helper" tag="SecretLang"/>
    </thing>
  <thing id="lvegepygmy" name="Vegepygmy" description="A wordless language of thumps, raps, and specialized pheromones utilized by the vegepygmies and other plant creatures." compset="Language" uniqueness="useronce">
    <usesource source="srcIntDark"/>
    </thing>
