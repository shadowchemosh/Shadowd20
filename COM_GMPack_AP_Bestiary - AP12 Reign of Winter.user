    <usesource source="PathRgnWin"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="User" tag="Activation"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="UserAdjust"/>
    <eval phase="Final" priority="10000"><![CDATA[doneif (field[abilActive].value = 0)

var val as number
val = (10 - field[actUser].value)

perform hero.child[xFastHeal].delete[Value.?]
if (val > 0) then
   perform hero.child[xFastHeal].assignstr["Value." & val]
endif]]></eval>
    </thing>
  <thing id="rauCHRebAu" name="Rebellious Aura" description="The herald&apos;s aura grants allies within 60 feet the effects of {i}good hope{/i} and a paladin&apos;s aura of courage. The effects of this ability are included in the herald&apos;s stat block." compset="RaceSpec">
