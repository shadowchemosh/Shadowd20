    <fieldval field="sEffect" value="See text"/>
    <fieldval field="sDuration" value="See text"/>
    <fieldval field="sSave" value="Will disbelief (if interacted with); varies; see text;"/>
    <fieldval field="sResist" value="Yes; see text"/>
