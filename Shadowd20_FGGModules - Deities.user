    <fieldval field="sTarget" value="One creature, or one nonmagical object of up to 100 cu. ft./level"/>
    <fieldval field="sDuration" value="See text"/>
    <fieldval field="sSave" value="Fortitude negates (object); see text"/>
    <fieldval field="sResist" value="Yes (object)"/>
