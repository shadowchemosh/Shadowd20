  <thing id="rauSwimChg" name="Swimming Charge" description="The creature can swim forward as a full-round action at the listed speed. It must move in a straight line while swimming." compset="RaceSpec" uniqueness="unique">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="SpecType" tag="Movement" name="Movement" abbrev="Movement"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="Final" priority="15000" index="2"><![CDATA[
      call ValueShow

      var swmSp as number
      swmSp = field[abValue].value * hero.child[xSwim].field[abValue].value

      ~ Use our value field as the speed at which we can swim
      field[abSumm].text = "Move in a straight line at " & swmSp & " ft."
      field[livename].text = field[name].text & " (" & swmSp & " ft.)"]]></eval>
    <eval phase="PostAttr" priority="9000">~ We do this because it allows detection and manipulation of a tag value
      ~ before the livename is set at Final 15000
      field[abValue].value = maximum(field[abValue].value, tagmax[Value.?])</eval>
    </thing>
