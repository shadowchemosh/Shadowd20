  <thing id="rcOrgWebDg" name="Webbed Digits" description="The ogrekin has thick, flexible webbing between its fingers and toes, gaining a swim speed equal to half its base land speed." compset="RaceCustom">
    <usesource source="Bestiary2"/>
    <usesource source="PathRise"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AllowRCust" tag="cfgOgrekin"/>
    <eval phase="PreLevel" priority="1000">
      ~ gain swim speed
      var swm as number
      var lnd as number
      lnd = hero.child[Speed].field[tSpeed].value
      swm = hero.child[xSwim].field[abValue].value

      hero.child[xSwim].field[abValue].value = maximum(swm,round(lnd/2,0,-1))</eval>
    </thing>
