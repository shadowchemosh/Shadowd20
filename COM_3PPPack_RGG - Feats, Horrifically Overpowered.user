  <thing id="fSGEschFoc" name="Eschew Foci" description="You have no need for idols and mirrors to cast spells.\n\n{b}Prerequisite{/b}: Eschew Materials\n\n{b}Benefit{/b}: You do not need to fulfill the focus or divine focus components of spells in order to cast them." compset="Feat" summary="You have no need for idols and mirrors to cast spells." uniqueness="useronce">
    <usesource source="SGG_HOverP"/>
    <tag group="fCategory" tag="Overpowerd"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Eschew Materials feat is required."><![CDATA[#hasfeat[fEschewMat] <> 0]]></exprreq>
    </thing>
  <thing id="fSGEschGst" name="Eschew Gestures" description="You have no need to wiggle your fingers to cast spells.\n\n{b}Prerequisite{/b}: Still Spell\n\n{b}Benefit{/b}: You do not need to fulfill the somatic components of spells in order to cast them. You effectively treat all the spells you cast as if they had been cast using the Still Spell metamagic feat, but the spell’s level does not change, and no extra time is required to cast it." compset="Feat" summary="You have no need to wiggle your fingers to cast spells." uniqueness="useronce">
    <usesource source="SGG_HOverP"/>
    <tag group="fCategory" tag="Overpowerd"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Still Spell feat is required."><![CDATA[#hasfeat[fStillSpl] <> 0]]></exprreq>
    </thing>
  <thing id="fSGEschInc" name="Eschew Incantations" description="You have no need to speak to cast spells.\n\n{b}Prerequisite{/b}: Silent Spell\n\n{b}Benefit{/b}: You do not need to fulfill the verbal components of spells in order to cast them. You effectively treat all the spells you cast as if they had been cast using the Silent Spell metamagic feat, but the spell’s level does not change, and no extra time is required to cast it." compset="Feat" summary="You have no need to speak to cast spells." uniqueness="useronce">
    <usesource source="SGG_HOverP"/>
    <tag group="fCategory" tag="Overpowerd"/>
